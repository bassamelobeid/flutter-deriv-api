{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : null,
   "description" : null,
   "type" : "object",
   "title" : null,
   "properties" : {
      "error" : {
         "properties" : {
            "message" : {
               "type" : "string",
               "pattern" : "Input validation failed: [_1]"
            },
            "code" : {
               "pattern" : "^InputValidationFailed",
               "type" : "string"
            },
            "details" : {
               "type" : "object",
               "[_1]" : {
                  "type" : "string",
                  "pattern" : "the schema does not allow additional properties"
               }
            }
         },
         "type" : "object",
         "required" : [
            "details",
            "message",
            "code"
         ]
      }
   }
}
