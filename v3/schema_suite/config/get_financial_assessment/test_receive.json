{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
        "required": "1"
    },
    "get_financial_assessment": {
      "type": "object",
      "properties": {
        "score": {
          "type": "integer",
          "pattern": "^[_1]$",
          "required": "1"
        },
        "account_opening_reason": {
          "type": "string",
          "pattern": "^Speculative$",
          "required":"1"
        },
        "account_turnover": {
          "type": "string",
          "pattern": "^Less than \\$25\\,000$",
          "required": "1"
        },
        "forex_trading_frequency": {
          "type": "string",
          "pattern": "^0\\-5 transactions in the past 12 months$",
          "required": "1"
        },
        "forex_trading_experience": {
          "type": "string",
          "pattern": "^Over 3 years$",
          "required": "1"
        },
        "indices_trading_frequency": {
          "type": "string",
          "pattern": "^40 transactions or more in the past 12 months$",
          "required": "1"
        },
        "indices_trading_experience": {
          "type": "string",
          "pattern": "^1\\-2 years$",
          "required": "1"
        },
        "commodities_trading_frequency": {
          "type": "string",
          "pattern": "^0\\-5 transactions in the past 12 months$",
          "required": "1"
        },
        "commodities_trading_experience": {
          "type": "string",
          "pattern": "^1\\-2 years$",
          "required": "1"
        },
        "stocks_trading_experience": {
          "type": "string",
          "pattern": "^1\\-2 years$",
          "required": "1"
        },
        "stocks_trading_frequency": {
          "type": "string",
          "pattern": "^0\\-5 transactions in the past 12 months$",
          "required": "1"
        },
        "other_derivatives_trading_frequency": {
          "type": "string",
          "pattern": "^0\\-5 transactions in the past 12 months$",
          "required": "1"
        },
        "other_derivatives_trading_experience": {
          "type": "string",
          "pattern": "^Over 3 years$",
          "required": "1"
        },
        "other_instruments_trading_frequency": {
          "type": "string",
          "pattern": "^6\\-10 transactions in the past 12 months$",
          "required": "1"
        },
        "other_instruments_trading_experience": {
          "type": "string",
          "pattern": "^Over 3 years$",
          "required": "1"
        },
        "employment_industry": {
          "type": "string",
          "pattern": "^Finance$",
          "required": "1"
        },
        "education_level": {
          "type": "string",
          "pattern": "^Secondary$",
          "required": "1"
        },
        "income_source": {
          "type": "string",
          "pattern": "^Self\\-Employed$",
          "required": "1"
        },
        "net_income": {
          "type": "string",
          "pattern": "^\\$25\\,000 \\- \\$50\\,000$",
          "required": "1"
        },
        "estimated_worth": {
          "type": "string",
          "pattern": "^\\$100\\,000 \\- \\$250\\,000$",
          "required": "1"
        }
      },
      "required": "1"
    },
    "msg_type": {
      "type": "string",
      "pattern": "^get_financial_assessment$",
      "required": "1"
    }
  }
}
