{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
        "required": "1"
    },
    "get_financial_assessment": {
      "type": "object",
      "properties": {
        "total_score": {
          "type": "integer",
          "pattern": "^[_1]$",
          "required": "1"
        },
        "cfd_score": {
          "type": "integer",
          "required": "1"
        },
        "financial_information_score": {
          "type": "integer",
          "required": "1"
        },
        "trading_score": {
          "type": "integer",
          "required": "1"
        },
        "account_turnover": {
          "type": "string",
          "pattern": "^Less than \\$25\\,000$"
        },
        "forex_trading_frequency": {
          "type": "string",
          "pattern": "^0\\-5 transactions in the past 12 months$"
        },
        "forex_trading_experience": {
          "type": "string",
          "pattern": "^Over 3 years$"
        },
        "binary_options_trading_frequency": {
          "type": "string",
          "pattern": "^40 transactions or more in the past 12 months$"
        },
        "binary_options_trading_experience": {
          "type": "string",
          "pattern": "^1\\-2 years$"
        },
        "cfd_trading_frequency": {
          "type": "string",
          "pattern": "^0\\-5 transactions in the past 12 months$"
        },
        "cfd_trading_experience": {
          "type": "string",
          "pattern": "^1\\-2 years$"
        },
        "other_instruments_trading_frequency": {
          "type": "string",
          "pattern": "^6\\-10 transactions in the past 12 months$"
        },
        "other_instruments_trading_experience": {
          "type": "string",
          "pattern": "^Over 3 years$"
        },
        "employment_industry": {
          "type": "string",
          "pattern": "^Finance$",
          "required": "1"
        },
        "education_level": {
          "type": "string",
          "pattern": "^Secondary$",
          "required": "1"
        },
        "income_source": {
          "type": "string",
          "pattern": "^Self\\-Employed$",
          "required": "1"
        },
        "net_income": {
          "type": "string",
          "pattern": "^\\$25\\,000 \\- \\$50\\,000$",
          "required": "1"
        },
        "estimated_worth": {
          "type": "string",
          "pattern": "^\\$100\\,000 \\- \\$250\\,000$",
          "required": "1"
        },
        "employment_status": {
          "type": "string",
          "pattern": "^Self-Employed$",
          "required": "1"
        },
        "source_of_wealth": {
          "type": "string",
          "pattern": "^Company Ownership$",
          "required": "1"
        }
      },
      "required": "1"
    },
    "msg_type": {
      "type": "string",
      "pattern": "^get_financial_assessment$",
      "required": "1"
    }
  }
}
