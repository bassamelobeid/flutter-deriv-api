{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "description" : null,
   "additionalProperties" : null,
   "type" : "object",
   "title" : null,
   "properties" : {
      "app_get" : {
         "required" : [
            "redirect_uri",
            "googleplay",
            "github",
            "app_id",
            "appstore",
            "verification_uri",
            "scopes",
            "name",
            "homepage",
            "app_markup_percentage"
         ],
         "properties" : {
            "redirect_uri" : {
               "type" : "string",
               "pattern" : "^https\\://test\\.example\\.com/redirect$"
            },
            "googleplay" : {
               "type" : "string",
               "pattern" : "^https://play\\.google\\.com/store/apps/details\\?id=test\\.app$"
            },
            "github" : {
               "type" : "string",
               "pattern" : "^https\\://github\\.com/test_org/app$"
            },
            "app_id" : {
               "type" : "integer",
               "enum" : [[_1]]
            },
            "appstore" : {
               "pattern" : "^https\\://itunes\\.apple\\.com/test_app$",
               "type" : "string"
            },
            "verification_uri" : {
               "pattern" : "^https\\://test\\.example\\.com/verify",
               "type" : "string"
            },
            "scopes" : {
               "items" : {
                  "type" : "string",
                  "pattern" : "^(read|trade)$"
               },
               "maxItem" : 2,
               "minItem" : 2,
               "type" : "array"
            },
            "name" : {
               "pattern" : "^Test Application$",
               "type" : "string"
            },
            "homepage" : {
               "pattern" : "^https\\://test\\.example\\.com/$",
               "type" : "string"
            },
            "app_markup_percentage" : {
               "type" : "number",
               "enum" : [1.5]
            }
         },
         "type" : "object"
      },
      "msg_type" : {
         "type" : "string",
         "pattern" : "^app_get$"
      },
      "echo_req" : {}
   }
}
