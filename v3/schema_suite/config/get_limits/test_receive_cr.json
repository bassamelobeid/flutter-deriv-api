{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "description" : null,
   "type" : "object",
   "additionalProperties" : null,
   "title" : null,
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^get_limits$"
      },
      "get_limits" : {
         "properties" : {
            "withdrawal_since_inception_monetary" : {
               "enum" : [
                  0
               ],
               "type" : "number"
            },
            "payout" : {
               "enum" : [
                  50000
               ],
               "type" : "number"
            },
            "remainder" : {
               "enum" : [
                  10000
               ],
               "type" : "number"
            },
            "num_of_days" : {
               "enum" : [
                  30
               ],
               "type" : "integer"
            },
            "withdrawal_for_x_days_monetary" : {
               "type" : "number",
               "enum" : [
                  0
               ]
            },
            "account_balance" : {
               "enum" : ["300000"],
               "type" : "number"
            },
            "num_of_days_limit" : {
               "enum" : [
                  10000
               ],
               "type" : "number"
            },
            "market_specific" : {
               "properties" : {
                  "commodities" : {
                     "items" : {
                        "type" : "object",
                        "properties" : {
                           "name" : {
                              "type" : "string",
                              "pattern" : "^Commodities$"
                           },
                           "turnover_limit" : {
                              "type" : "number",
                              "enum" : [
                                 50000
                              ]
                           }
                        }
                     },
                     "type" : "array"
                  },
                  "indices" : {
                     "type" : "array",
                     "items" : {
                        "type" : "object",
                        "properties" : {
                           "name" : {
                              "type" : "string",
                              "pattern" : "^Indices$"
                           },
                           "turnover_limit" : {
                              "enum" : [
                                 100000
                              ],
                              "type" : "number"
                           }
                        }
                     }
                  },
                  "volidx" : {
                     "items" : {
                        "type" : "object",
                        "properties" : {
                           "turnover_limit" : {
                              "type" : "number",
                              "enum" : [
                                 500000
                              ]
                           },
                           "name" : {
                              "type" : "string",
                              "pattern" : "^Volatility Indices$"
                           }
                        }
                     },
                     "type" : "array"
                  },
                  "forex" : {
                     "items" : {
                        "properties" : {
                           "turnover_limit" : {
                              "enum" : [
                                 100000,
                                 50000
                              ],
                              "type" : "number"
                           },
                           "name" : {
                              "type" : "string",
                              "pattern" : "^(Major Pairs|Minor Pairs|Smart FX)$"
                           }
                        },
                        "type" : "object"
                     },
                     "type" : "array"
                  }
               },
               "type" : "object",
               "required" : [
                  "commodities",
                  "indices",
                  "volidx",
                  "forex"
               ]
            },
            "lifetime_limit" : {
               "enum" : [
                  10000
               ],
               "type" : "number"
            },
            "open_positions" : {
               "enum" : [
                  100
               ],
               "type" : "integer"
            }
         },
         "type" : "object",
         "required" : [
            "withdrawal_since_inception_monetary",
            "payout",
            "remainder",
            "num_of_days",
            "withdrawal_for_x_days_monetary",
            "account_balance",
            "num_of_days_limit",
            "lifetime_limit",
            "open_positions"
         ]
      },
      "echo_req" : {}
   }
}
