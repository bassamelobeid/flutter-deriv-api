{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "payout_currencies",
      "msg_type",
      "echo_req"
   ],
   "type" : "object",
   "additionalProperties" : null,
   "title" : null,
   "description" : null,
   "properties" : {
      "payout_currencies" : {
         "items" : {
            "type" : "string",
            "pattern" : "^(USD|EUR|GBP|AUD|BTC|LTC|BCH|ETH|UST|USB|IDK)$"
         },
         "minItems" : 7,
         "type" : "array"
      },
      "echo_req" : {
         "type" : "object",
         "required" : ["payout_currencies"],
         "payout_currencies" : {
            "type" : "number"
         }
      },
      "msg_type" : {
         "type" : "string"
      }
   }
}
