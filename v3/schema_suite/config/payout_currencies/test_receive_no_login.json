{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
      "type": "object",
      "required": "1",
      "payout_currencies": {
        "type": "number",
        "required": "1"
      }
    },
    "payout_currencies": {
      "type": "array",
      "minItems": 7,
      "required": "1",
      "items": {
        "type": "string",
        "pattern": "^(USD|EUR|GBP|AUD|BTC|LTC|BCH|ETH|UST)$",
        "required": "1"
      }
    },
    "msg_type":{
        "type":"string",
        "required": "1"
   }
  }
}
