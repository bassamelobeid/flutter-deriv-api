{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "additionalProperties" : null,
   "type" : "object",
   "title" : null,
   "description" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "buy" : {
         "properties" : {
            "payout" : {
               "type" : "number",
               "pattern" : "^[_3]"
            },
            "purchase_time" : {
               "type" : "integer",
               "pattern" : "^\\d{10}$"
            },
            "shortcode" : {
               "type" : "string"
            },
            "contract_id" : {
               "type" : "integer"
            },
            "start_time" : {
               "type" : "integer",
               "pattern" : "^\\d{10}$"
            },
            "longcode" : {
               "type" : "string"
            },
            "balance_after" : {
               "type" : "number"
            },
            "buy_price" : {
               "pattern" : "^[_2]",
               "type" : "number"
            },
            "transaction_id" : {
               "type" : "integer"
            }
         },
         "type" : "object",
         "required" : [
            "payout",
            "purchase_time",
            "shortcode",
            "contract_id",
            "start_time",
            "longcode",
            "balance_after",
            "buy_price",
            "transaction_id"
         ]
      },
      "msg_type" : {
         "pattern" : "^buy$",
         "type" : "string"
      },
      "echo_req" : {}
   }
}
