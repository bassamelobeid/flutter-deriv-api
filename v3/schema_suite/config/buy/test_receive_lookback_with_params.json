{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "title" : null,
   "additionalProperties" : null,
   "type" : "object",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^buy$"
      },
      "echo_req" : {},
      "buy" : {
         "required" : [
            "longcode",
            "balance_after",
            "start_time",
            "transaction_id",
            "buy_price",
            "purchase_time",
            "payout",
            "shortcode",
            "contract_id"
         ],
         "properties" : {
            "longcode" : {
               "type" : "string"
            },
            "balance_after" : {
               "type" : "number"
            },
            "start_time" : {
               "type" : "integer",
               "pattern" : "^\\d{10}$"
            },
            "transaction_id" : {
               "type" : "integer"
            },
            "buy_price" : {
               "type" : "number",
               "pattern" : "^[_2]"
            },
            "purchase_time" : {
               "pattern" : "^\\d{10}$",
               "type" : "integer"
            },
            "payout" : {
               "pattern" : "^[_3]",
               "type" : "number"
            },
            "shortcode" : {
               "type" : "string"
            },
            "contract_id" : {
               "type" : "integer"
            }
         },
         "type" : "object"
      }
   }
}
