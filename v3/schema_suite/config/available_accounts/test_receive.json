{
    "$schema":"http://json-schema.org/draft-04/schema#",
    "required":[
       "msg_type",
       "echo_req"
    ],
    "title":null,
    "additionalProperties":null,
    "type":"object",
    "description":null,
    "properties":{
       "msg_type":{
          "type":"string",
          "pattern":"^available_accounts$"
       },
       "echo_req":{
          
       },
       "available_accounts":{
          "type":"object",
          "additionalProperties":false,
          "required":[
             "wallets"
          ],
          "properties":{
             "wallets":{
                "type":"array",
                "items":{
                   "type":"object",
                   "additionalProperties":false,
                   "required":[
                      "account_type",
                      "currency",
                      "landing_company"
                   ],
                   "properties":{
                      "account_type":{
                         "description":"Account type of wallet",
                         "type":"string",
                         "enum":[
                            "doughflow",
                            "crypto",
                            "paymentagent",
                            "paymentagent_client",
                            "p2p"
                         ]
                      },
                      "currency":{
                         "type":"string"
                      },
                      "landing_company":{
                         "type":"string"
                      }
                   }
                }
             }
          }
       }
    }
 }
 