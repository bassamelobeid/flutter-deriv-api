{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "title" : null,
   "additionalProperties" : null,
   "type" : "object",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "pattern" : "^proposal$",
         "type" : "string"
      },
      "echo_req" : {},
      "proposal" : {
         "properties" : {
            "longcode" : {
               "pattern" : "^[_6]$",
               "type" : "string"
            },
            "spot_time" : {
               "type" : "number",
               "pattern" : "^\\d+$"
            },
            "display_value" : {
               "pattern" : "^[_8]$",
               "type" : "number"
            },
            "id" : {
               "type" : "string",
               "pattern" : "[\\d\\w-]+$"
            },
            "date_start" : {
               "type" : "number",
               "pattern" : "^\\d+$"
            },
            "payout" : {
               "type" : "number",
               "pattern" : "^0$"
            },
            "spot" : {
               "pattern" : "^[_9]$",
               "type" : "number"
            },
            "ask_price" : {
               "type" : "number",
               "pattern" : "^[_7]$"
            }
         },
         "type" : "object",
         "required" : [
            "longcode",
            "spot_time",
            "display_value",
            "id",
            "date_start",
            "payout",
            "spot",
            "ask_price"
         ]
      }
   }
}
