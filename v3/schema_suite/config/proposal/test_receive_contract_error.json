{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "description" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^proposal$"
      },
      "error" : {
         "properties" : {
            "message" : {
               "type" : "string",
               "pattern" : "^Cannot validate contract."
            },
            "code" : {
               "type" : "string",
               "pattern" : "^ContractBuyValidationError$"
            }
         },
         "type" : "object",
         "required" : [
            "message",
            "code"
         ]
      },
      "echo_req" : {}
   }
}
