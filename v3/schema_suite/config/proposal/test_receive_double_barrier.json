{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "title" : null,
   "additionalProperties" : null,
   "type" : "object",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "proposal" : {
         "required" : [
            "display_value",
            "id",
            "longcode",
            "spot_time",
            "ask_price",
            "spot",
            "payout",
            "date_start"
         ],
         "type" : "object",
         "properties" : {
            "display_value" : {
               "type" : "number",
               "pattern" : "^[_9]$"
            },
            "id" : {
               "pattern" : "[\\d\\w-]+$",
               "type" : "string"
            },
            "longcode" : {
               "type" : "string",
               "pattern" : "^[_7]$"
            },
            "spot_time" : {
               "pattern" : "^\\d+$",
               "type" : "number"
            },
            "ask_price" : {
               "pattern" : "^[_8]$",
               "type" : "number"
            },
            "spot" : {
               "type" : "number",
               "pattern" : "^[_10]$"
            },
            "payout" : {
               "pattern" : "^100$",
               "type" : "number"
            },
            "date_start" : {
               "type" : "number",
               "pattern" : "^\\d+$"
            }
         }
      },
      "msg_type" : {
         "type" : "string",
         "pattern" : "^proposal$"
      },
      "echo_req" : {}
   }
}
