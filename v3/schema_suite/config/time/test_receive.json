{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Server time",
  "properties": {
    "echo_req": {
      "required": "1"
    },
    "time": {
      "type": "integer",
      "pattern": "^[0-9]{10}$",
      "required": "1"
    },
    "msg_type": {
        "type": "string",
        "pattern": "^time$",
        "required": "1"
    }
  }
}
