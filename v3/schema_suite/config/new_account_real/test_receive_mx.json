{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "additionalProperties" : null,
   "type" : "object",
   "title" : null,
   "description" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^new_account_real$"
      },
      "echo_req" : {},
      "new_account_real" : {
         "required" : [
            "landing_company",
            "landing_company_shortcode",
            "oauth_token",
            "client_id"
         ],
         "properties" : {
            "landing_company" : {
               "type" : "string",
               "pattern" : "^Deriv \\(MX\\) Ltd$"
            },
            "landing_company_shortcode" : {
               "pattern" : "^iom$",
               "type" : "string"
            },
            "oauth_token" : {
               "type" : "string",
               "pattern" : "^.{32}$"
            },
            "client_id" : {
               "type" : "string",
               "pattern" : "^MX\\d+$"
            }
         },
         "type" : "object"
      }
   }
}
