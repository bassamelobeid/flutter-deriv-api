{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "description" : null,
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^new_account_real$"
      },
      "echo_req" : {},
      "new_account_real" : {
         "type" : "object",
         "properties" : {
            "client_id" : {
               "pattern" : "^MLT\\d+$",
               "type" : "string"
            },
            "oauth_token" : {
               "type" : "string",
               "pattern" : "^.{32}$"
            },
            "landing_company_shortcode" : {
               "pattern" : "^malta$",
               "type" : "string"
            },
            "landing_company" : {
               "pattern" : "^Binary \\(Europe\\) Ltd$",
               "type" : "string"
            }
         },
         "required" : [
            "client_id",
            "oauth_token",
            "landing_company_shortcode",
            "landing_company"
         ]
      }
   }
}
