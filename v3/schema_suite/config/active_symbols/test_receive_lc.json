{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "active_symbols",
      "echo_req",
      "msg_type"
   ],
   "description" : null,
   "title" : null,
   "additionalProperties" : null,
   "type" : "object",
   "properties" : {
      "active_symbols" : {
         "items" : {
             "required": ["submarket", "market_display_name", "is_trading_suspended", "exchange_is_open", "submarket_display_name", "display_name", "pip", "symbol", "market", "symbol_type"], 
            "properties" : {
               "submarket" : {
                  "pattern" : "^[_3]$",
                  "type" : "string"
               },
               "market_display_name" : {
                  "type" : "string"
               },
               "is_trading_suspended" : {
                  "pattern" : "^(0|1)$",
                  "type" : "integer"
               },
               "exchange_is_open" : {
                  "pattern" : "^(0|1)$",
                  "type" : "integer"
               },
               "submarket_display_name" : {
                  "type" : "string"
               },
               "display_name" : {
                  "type" : "string"
               },
               "pip" : {
                  "type" : "number",
                  "minimum": 0.00001,
                  "maximum": 0.01
               },
               "symbol" : {
                  "type" : "string"
               },
               "market" : {
                  "type" : "string",
                  "pattern" : "^[_2]$"
               },
               "symbol_type" : {
                  "type" : "string"
               }
            },
            "type" : "object"
         },
         "type" : "array"
      },
      "echo_req" : {},
      "msg_type" : {
         "type" : "string",
         "pattern" : "^active_symbols$"
      }
   }
}
