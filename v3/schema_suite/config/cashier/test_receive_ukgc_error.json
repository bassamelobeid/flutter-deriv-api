{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "description" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "pattern" : "^cashier$",
         "type" : "string"
      },
      "error" : {
         "required" : [
            "message",
            "code"
         ],
         "type" : "object",
         "properties" : {
            "message" : {
               "type" : "string"
            },
            "code" : {
               "pattern" : "^ASK_UK_FUNDS_PROTECTION$",
               "type" : "string"
            }
         }
      },
      "echo_req" : {}
   }
}
