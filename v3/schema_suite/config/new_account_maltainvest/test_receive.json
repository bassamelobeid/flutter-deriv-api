{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "echo_req": {
            "required": "1"
        },
        "new_account_maltainvest": {
            "type": "object",
            "properties": {
                "client_id": {
                    "type": "string",
                    "pattern": "^MF[0-9]+$",
                    "required": "1"
                },
                "landing_company": {
                    "type": "string",
                    "pattern": "^Binary Investments \\(Europe\\) Ltd$",
                    "required": "1"
                },
                "landing_company_shortcode": {
                    "type": "string",
                    "pattern": "^maltainvest$",
                    "required": "1"
                },
                "oauth_token": {
                    "type": "string",
                    "pattern": "^.{32}$",
                    "required": "1"
                }
            },
            "required": "1"
        },
        "msg_type": {
            "type": "string",
            "pattern": "^new_account_maltainvest$",
            "required": "1"
        }
    }
}
