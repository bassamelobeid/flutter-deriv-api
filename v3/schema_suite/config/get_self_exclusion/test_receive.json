            },
            "max_balance" : {
               "type" : "integer",
               "pattern" : "^100000$"
            },
            "max_30day_turnover" : {
               "type" : "integer",
               "pattern" : "^1000$"
            },
            "max_7day_turnover" : {
               "type" : "integer",
               "pattern" : "^1000$"
            },
            "max_turnover" : {
               "pattern" : "^1000$",
               "type" : "integer"
            },
            "session_duration_limit" : {
               "type" : "integer",
