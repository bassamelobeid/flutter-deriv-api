{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
        "required": "1"
    },
    "get_self_exclusion": {
      "type": "object",
      "properties":{
        "max_balance":{
          "type": "integer",
          "pattern": "^100000$",
          "required": "1"
        },
        "max_turnover":{
          "type": "integer",
          "pattern": "^1000$",
          "required": "1"
        },
        "max_losses":{
          "type": "integer",
          "pattern": "^100000$",
          "required": "1"
        },
        "max_7day_turnover":{
          "type": "integer",
          "pattern": "^1000$",
          "required": "1"
        },
        "max_7day_losses":{
          "type": "integer",
          "pattern": "^100000$",
          "required": "1"
        },
        "max_30day_turnover":{
          "type": "integer",
          "pattern": "^1000$",
          "required": "1"
        },
        "max_30day_losses":{
          "type": "integer",
          "pattern": "^100000$",
          "required": "1"
        },
        "max_open_bets":{
          "type": "integer",
          "pattern": "^50$",
          "required": "1"
        },
        "session_duration_limit":{
          "type": "integer",
          "pattern": "^3600$",
          "required": "1"
        }
      },
      "required": "1"
    },
    "msg_type": {
      "type": "string",
      "pattern": "^get_self_exclusion$",
      "required": "1"
    }
  }
}
