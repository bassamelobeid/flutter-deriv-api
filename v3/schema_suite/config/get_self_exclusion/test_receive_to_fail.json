{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "description" : null,
   "properties" : {
      "get_self_exclusion" : {
         "required" : [
            "max_losses",
            "max_7day_losses",
            "session_duration_limit",
            "max_turnover",
            "max_open_bets",
            "max_30day_losses",
            "max_balance",
            "max_30day_turnover",
            "max_7day_turnover"
         ],
         "properties" : {
            "max_losses" : {
               "pattern" : "^5000$",
               "type" : "integer"
            },
            "max_7day_losses" : {
               "pattern" : "^400$",
               "type" : "integer"
            },
            "session_duration_limit" : {
               "pattern" : "^36120$",
               "type" : "integer"
            },
            "max_turnover" : {
               "pattern" : "^3000$",
               "type" : "integer"
            },
            "max_open_bets" : {
               "pattern" : "^60$",
               "type" : "integer"
            },
            "max_30day_losses" : {
               "pattern" : "^50100$",
               "type" : "integer"
            },
            "max_balance" : {
               "type" : "integer",
               "pattern" : "^1000$"
            },
            "max_30day_turnover" : {
               "pattern" : "^7000$",
               "type" : "integer"
            },
            "max_7day_turnover" : {
               "pattern" : "^20$",
               "type" : "integer"
            }
         },
         "type" : "object"
      },
      "msg_type" : {
         "type" : "string",
         "pattern" : "^get_self_exclusion$"
      },
      "echo_req" : {}
   }
}
