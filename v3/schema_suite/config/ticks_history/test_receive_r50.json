{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "echo_req",
      "msg_type"
   ],
   "title" : null,
   "additionalProperties" : null,
   "type" : "object",
   "description" : null,
   "properties" : {
      "echo_req" : {},
      "msg_type" : {
         "type" : "string",
         "pattern" : "^history$"
      },
      "history" : {
         "required" : [
            "times",
            "prices"
         ],
         "type" : "object",
         "properties" : {
            "times" : {
               "type" : [
                  "array",
                  "null"
               ],
               "minItems" : 10,
               "items" : {
                  "pattern" : "^\\d{10}$",
                  "type" : "integer"
               },
               "maxItems" : 10
            },
            "prices" : {
               "items" : {
                  "pattern" : "^\\d+\\.\\d+$",
                  "type" : "string"
               },
               "maxItems" : 10,
               "minItems" : 10,
               "type" : [
                  "array",
                  "null"
               ]
            }
         }
      }
   }
}
