{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
      "required": "1"
    },
    "history": {
      "type": "object",
      "required": "1",
      "properties": {
        "times": {
          "type": ["array","null"],
          "maxItems": 10,
          "minItems": 10,
          "required": "1",
          "items":
            {
              "type": "integer",
              "pattern": "^[0-9]{10}$",
              "required": "1"
            }
        },
        "prices": {
          "type": ["array","null"],
          "maxItems": 10,
          "minItems": 10,
          "required": "1",
          "items":
            {
              "type": "string",
              "pattern": "^[0-9]+\\.[0-9]+$",
              "required": "1"
            }
        }
      }
    },
    "msg_type": {
      "type": "string",
      "pattern": "^history$",
      "required": "1"
    }
  }
}
