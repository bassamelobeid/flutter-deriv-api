{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "additionalProperties" : null,
   "type" : "object",
   "title" : null,
   "required" : [
      "echo_req",
      "msg_type"
   ],
   "properties" : {
      "reality_check" : {
         "required" : [
            "sell_amount",
            "start_time",
            "buy_count",
            "sell_count",
            "potential_profit",
            "loginid",
            "open_contract_count",
            "currency",
            "buy_amount"
         ],
         "type" : "object",
         "properties" : {
            "sell_amount" : {
               "type" : "number"
            },
            "start_time" : {
               "type" : "integer"
            },
            "buy_count" : {
               "type" : "integer"
            },
            "sell_count" : {
               "type" : "integer"
            },
            "potential_profit" : {
               "type" : "number"
            },
            "loginid" : {
               "pattern" : "^MLT\\d+$",
               "type" : "string"
            },
            "open_contract_count" : {
               "type" : "integer"
            },
            "currency" : {
               "type" : "string",
               "pattern" : "^[_1]$"
            },
            "buy_amount" : {
               "type" : "number"
            }
         }
      },
      "echo_req" : {},
      "msg_type" : {
         "type" : "string",
         "pattern" : "^reality_check$"
      }
   }
}
