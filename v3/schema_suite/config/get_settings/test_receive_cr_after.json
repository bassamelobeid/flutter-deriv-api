{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "type" : "object",
   "additionalProperties" : null,
   "title" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "get_settings" : {
         "type" : "object",
         "required" : [
            "address_state",
            "email_consent",
            "client_tnc_status",
            "date_of_birth",
            "country",
            "email",
            "address_postcode",
            "address_line_1",
            "last_name",
            "address_line_2",
            "phone",
            "first_name",
            "account_opening_reason",
            "is_authenticated_payment_agent",
            "salutation",
            "country_code",
            "address_city",
            "non_pep_declaration",
            "immutable_fields",
            "preferred_language",
            "trading_hub",
            "phone_number_verification"
         ],
         "properties" : {
            "address_state" : {
               "pattern" : "^BT$",
               "type" : "string"
            },
            "email_consent" : {
               "enum" : [0,1],
               "type" : "integer"
            },
            "client_tnc_status" : {
               "type" : "string",
               "pattern" : "^Version \\d{2} \\d{4}-\\d{2}-\\d{2}$"
            },
            "date_of_birth" : {
               "type" : "integer",
               "enum" : [318124800]
            },
            "country" : {
               "pattern" : "^Indonesia$",
               "type" : "string"
            },
            "place_of_birth" : {
               "type" : "string"
            },
            "tax_identification_number" : {
               "type" : ["string", "null"] 
            },
            "email" : {
               "pattern" : "test1@binary.com",
               "type" : "string"
            },
            "tax_residence" : {
               "type" : ["string", "null"] 
            },
            "address_postcode" : {
               "pattern" : "^123456$",
               "type" : "string"
            },
            "address_line_1" : {
               "type" : "string",
               "pattern" : "^Test Address Line 1$"
            },
            "last_name" : {
               "type" : "string",
               "pattern" : "^Pan$"
            },
            "citizen" : {
               "type" : "string"
            },
            "address_line_2" : {
               "type" : "string",
               "pattern" : "^Test Address Line 2$"
            },
            "phone" : {
               "type" : ["string", "null"]
            },
            "first_name" : {
               "pattern" : "^Peter$",
               "type" : "string"
            },
            "account_opening_reason" : {
               "pattern" : "^Speculative",
               "type" : "string"
            },
            "is_authenticated_payment_agent" : {
               "pattern" : "^0$",
               "type" : "integer"
            },
            "salutation" : {
               "pattern" : "^Mr$",
               "type" : "string"
            },
            "country_code" : {
               "type" : "string",
               "pattern" : "^id$"
            },
            "address_city" : {
               "type" : "string",
               "pattern" : "^Test City$"
            },
            "non_pep_declaration": {
                "type": "integer",
                "enum": [
                    1
                ]
            },
            "fatca_declaration": {
               "type": [
                  "integer",
                  "null"
               ]
            },
            "immutable_fields" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            },
            "preferred_language" : {
               "type" : "string",
               "pattern" : "^[A-Z]{0,2}$|^[A-Z]{2}_[A-Z]{2}$"
            },
            "trading_hub" : {
               "type" : "integer",
               "enum" : [
                  0,
                  1
               ]
            },
            "phone_number_verification": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "verified"
                ],
                "properties": {
                    "next_attempt": {
                        "type": "integer"
                    },
                    "verified": {
                        "type": "integer",
                        "enum": [
                            0,
                            1
                        ]
                    }
                }
            }
         }
      },
      "msg_type" : {
         "pattern" : "^get_settings$",
         "type" : "string"
      },
      "echo_req" : {}
   }
}
