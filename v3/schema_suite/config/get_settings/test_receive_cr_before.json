{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "description" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^get_settings$"
      },
      "get_settings" : {
         "required" : [
            "address_line_1",
            "last_name",
            "email",
            "address_postcode",
            "email_consent",
            "date_of_birth",
            "client_tnc_status",
            "country",
            "address_state",
            "address_city",
            "country_code",
            "salutation",
            "is_authenticated_payment_agent",
            "phone",
            "address_line_2",
            "first_name",
            "account_opening_reason"
         ],
         "properties" : {
            "address_line_1" : {
               "type" : "string",
               "pattern" : "^20 Broadway Av$"
            },
            "last_name" : {
               "type" : "string",
               "pattern" : "^Pan$"
            },
            "email" : {
               "pattern" : "test@binary.com",
               "type" : "string"
            },
            "address_postcode" : {
               "type" : "string",
               "pattern" : "^3002$"
            },
            "email_consent" : {
               "enum" : [0,1],
               "type" : "integer"
            },
            "date_of_birth" : {
               "type" : "integer",
               "enum" : [318124800]
            },
            "client_tnc_status" : {
               "type" : "string",
               "pattern" : "^Version \\d{2} \\d{4}-\\d{2}-\\d{2}$"
            },
            "country" : {
               "type" : "string",
               "pattern" : "^Indonesia$"
            },
            "address_state" : {
               "pattern" : "^BT$",
               "type" : "string"
            },
            "address_city" : {
               "pattern" : "^Melbourne$",
               "type" : "string"
            },
            "country_code" : {
               "type" : "string",
               "pattern" : "^id$"
            },
            "salutation" : {
               "type" : "string",
               "pattern" : "^Mr$"
            },
            "is_authenticated_payment_agent" : {
               "type" : "integer",
               "pattern" : "^0$"
            },
            "phone" : {
               "pattern" :"^\\+?((-|\\s)*[0-9]){8,35}$",
               "type" : "string"
            },
            "address_line_2" : {
               "type" : "string",
               "pattern" : "^East Melbourne VIC$"
            },
            "first_name" : {
               "type" : "string",
               "pattern" : "^Peter$"
            },
            "account_opening_reason" : {
               "pattern" : "^Speculative",
               "type" : "string"
            },
            "citizen" : {
               "type" : "string"
            }
         },
         "type" : "object"
      },
      "echo_req" : {}
   }
}
