        "allow_copiers": {
          "type" : "integer",
          "enum" : [
            0,
            1
          ]
        },
        "has_secret_answer" : {
          "type" : "integer",
          "enum" : [
            0,
            1
          ]
        },
        "citizen" : {
          "type" : "string"
