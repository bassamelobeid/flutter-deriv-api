{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "type" : "object",
   "additionalProperties" : null,
   "title" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "get_settings" : {
         "required" : [
            "email_consent",
            "country",
            "email",
            "country_code"
         ],
         "type" : "object",
         "properties" : {
            "email_consent" : {
               "pattern" : "^(0|1)$",
               "type" : "integer"
            },
            "country" : {
               "type" : "string",
               "pattern" : "^[_1]$"
            },
            "email" : {
               "type" : "string"
            },
            "country_code" : {
               "type" : "string",
               "pattern" : "^[_2]$"
            }
         }
      },
      "msg_type" : {
         "pattern" : "^get_settings$",
         "type" : "string"
      },
      "echo_req" : {}
   }
}
