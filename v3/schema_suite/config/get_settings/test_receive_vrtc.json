    "get_settings":{
      "type": "object",
      "additionalProperties": null,
      "required" : [
        "first_name",
        "date_of_birth",
        "country",
        "account_opening_reason",
        "address_city",
        "address_state",
        "country_code",
        "phone",
        "address_line_1",
        "address_postcode",
        "address_line_2",
        "email_consent",
        "email",
        "last_name",
        "salutation",
        "is_authenticated_payment_agent",
        "client_tnc_status",
        "non_pep_declaration",
        "fatca_declaration",
        "preferred_language",
        "trading_hub",
        "phone_number_verification",
        "allow_copiers",
        "citizen",
        "has_secret_answer",
        "dxtrade_user_exception",
        "feature_flag",
        "place_of_birth",
        "request_professional_status",
        "residence",
        "tax_identification_number",
        "tax_residence",
