          "type" : "integer"
        },
        "account_opening_reason" : {
          "type" : ["string", "null"]
        },
        "address_postcode" : {
          "type" : "string"
        },
        "first_name" : {
          "type" : "string"
        },
        "country" : {
          "pattern" : "^Indonesia$",
          "type" : "string"
        },
        "email" : {
          "pattern" : "test1@binary.com",
          "type" : "string"
        },
        "address_line_2" : {
          "type" : "string"
        },
        "client_tnc_status" : {
          "type" : "string"
