          "enum": [
            0,
            1
          ]
        },
        "fatca_declaration": {
          "description": "Indicates client's self-declaration of FATCA.",
          "type": "integer",
          "enum": [
            0,
            1
          ]
        },
        "immutable_fields" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "preferred_language" : {
          "type" : "string",
          "pattern": "^[A-Z]{0,2}$|^[A-Z]{2}_[A-Z]{2}$"
        },
        "trading_hub" : {
          "type" : "integer",
          "enum" : [
            0,
            1
          ]
        },
        "phone_number_verification": {
          "type": "object",
          "properties" : {
            "verified" : {
              "type" : "integer",
