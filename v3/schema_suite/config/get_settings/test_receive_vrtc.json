   {
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "type" : "object",
   "additionalProperties" : null,
   "title" : null,
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "get_settings" : {
         "type" : "object",
         "additionalProperties" : null,
         "required" : [
            "email_consent",
            "country",
            "email",
            "country_code",
            "user_hash",
            "immutable_fields",
            "preferred_language",
            "trading_hub"
         ],
         "properties" : {
            "email_consent" : {
               "pattern" : "^(0|1)$",
               "type" : "integer"
            },
            "country" : {
               "type" : "string",
               "pattern" : "^[_1]$"
            },
            "residence" : {
               "type" : "string",
               "pattern" : "^[_1]$"
            },
            "email" : {
               "type" : "string"
            },
            "country_code" : {
               "type" : "string",
               "pattern" : "^[_2]$"
            },
            "user_hash": {
               "type": "string"
            },
            "immutable_fields" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            },
            "preferred_language" : {
               "type" : "string",
               "pattern" : "^[A-Z]{0,2}$|^[A-Z]{2}_[A-Z]{2}$"
            },
            "feature_flag" : {
               "type" : "object",
               "properties": {
                  "wallet": {
                     "type": "integer",
                     "enum": [
                        0,
                        1
                     ]
                  }
               }
            },
            "cooling_off_expiration_date": {
               "type": [
                   "null",
                   "integer"
               ]
           },
            "trading_hub" : {
               "type" : "integer",
               "enum" : [
                  0,
                  1
               ]
            }
         }
      },
      "msg_type" : {
         "pattern" : "^get_settings$",
         "type" : "string"
      },
      "echo_req" : {}
   }
}
