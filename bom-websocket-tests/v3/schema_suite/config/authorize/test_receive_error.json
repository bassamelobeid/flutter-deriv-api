{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : null,
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "required" : [
      "msg_type",
      "echo_req"
   ],
   "properties" : {
      "msg_type" : {
         "type" : "string",
         "pattern" : "^authorize$"
      },
      "error" : {
         "required" : [
            "message",
            "code"
         ],
         "properties" : {
            "message" : {
               "type" : "string"
            },
            "code" : {
               "type" : "string",
               "pattern" : "^InvalidToken$"
            }
         },
         "type" : "object"
      },
      "echo_req" : {}
   }
}
