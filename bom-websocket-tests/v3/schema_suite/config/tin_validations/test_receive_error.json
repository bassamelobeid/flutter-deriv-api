{
    "$schema" : "http://json-schema.org/draft-04/schema#",
    "additionalProperties" : null,
    "title" : null,
    "type" : "object",
    "description" : null,
    "required" : [
       "echo_req",
       "msg_type"
    ],
    "properties" : {
       "echo_req" : {},
       "msg_type" : {
          "pattern" : "^tin_validations$",
          "type" : "string"
       },
       "error" : {
          "type" : "object",
          "properties" : {
             "code" : {
                "pattern" : "^InputValidationFailed$",
                "type" : "string"
             },
             "message" : {
                "type" : "string"
             }
          },
          "required" : [
             "code",
             "message"
          ]
       }
    }
}
