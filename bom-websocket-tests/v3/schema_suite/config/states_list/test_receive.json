{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "additionalProperties" : null,
   "title" : null,
   "type" : "object",
   "description" : null,
   "required" : [
      "states_list",
      "echo_req",
      "msg_type"
   ],
   "properties" : {
      "states_list" : {
         "items" : {
            "properties" : {
                "required": ["value", "text"],
               "value" : {
                  "pattern" : "^.{2}$",
                  "type" : "string"
               },
               "text" : {
                  "type" : "string"
               }
            },
            "type" : "object"
         },
         "type" : "array",
         "minItems" : 1
      },
      "echo_req" : {},
      "msg_type" : {
         "pattern" : "^states_list$",
         "type" : "string"
      }
   }
}
