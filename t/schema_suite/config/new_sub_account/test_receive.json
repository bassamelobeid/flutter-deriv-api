{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "client_id": {
            "type": "string",
            "pattern": "^CR\\d+$",
            "required": "1"
        },
        "landing_company": {
            "type": "string",
            "pattern": "^Binary \\(C\\.R\\.\\) S\\.A\\.$",
            "required": "1"
        },
        "landing_company_shortcode": {
            "type": "string",
            "pattern": "^costarica$",
            "required": "1"
        }
    }
}
