{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "title" : null,
   "additionalProperties" : false,
   "type" : "object",
   "description" : null,
   "required" : [
      "email",
      "client_id",
      "oauth_token",
      "balance",
      "currency",
      "stash"
   ],
   "properties" : {
      "stash" : {
         "type" : "object"
      },
      "email" : {
         "pattern" : "^[_3]$",
         "type" : "string"
      },
      "balance" : {
         "pattern" : "^10000.00$",
         "type" : "string"
      },
      "client_id" : {
         "pattern" : "^VRTC\\d+$",
         "type" : "string"
      },
      "currency" : {
         "pattern" : "^USD$",
         "type" : "string"
      },
      "oauth_token" : {
         "type" : "string"
      }
   }
}
