{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "client_id": {
            "type": "string",
            "pattern": "^CR\\d+$",
            "required": "1"
        },
        "landing_company": {
            "type": "string",
            "pattern": "^Binary \\(SVG\\) Ltd\\.$",
            "required": "1"
        },
        "landing_company_shortcode": {
            "type": "string",
            "pattern": "^svg$",
            "required": "1"
        },
        "oauth_token": {
            "type": "string",
            "pattern": "^.{32}$",
            "required": "1"
        }
    }
}
