{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "title" : null,
   "additionalProperties" : false,
   "description" : null,
   "type" : "object",
   "required" : [
      "client_id",
      "oauth_token",
      "landing_company_shortcode",
      "landing_company",
      "stash"
   ],
   "properties" : {
      "landing_company" : {
         "type" : "string",
         "pattern" : "^Binary \\(IOM\\) Ltd$"
      },
      "oauth_token" : {
         "type" : "string",
         "pattern" : "^.{32}$"
      },
      "landing_company_shortcode" : {
         "type" : "string",
         "pattern" : "^iom$"
      },
      "stash" : {
         "type" : "object"
      },
      "client_id" : {
         "pattern" : "^MX\\d+$",
         "type" : "string"
      }
   }
}
