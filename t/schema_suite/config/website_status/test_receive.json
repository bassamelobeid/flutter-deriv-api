{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "clients_country": {
            "type": "string"
        },
        "terms_conditions_version": {
            "type": "string",
            "required": "1"
        },
        "supported_languages": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "api_call_limits": {
            "type": "object",
            "required": "1",
            "properties": {
                "max_proposal_subscription": {
                    "type": "object",
                    "required": "1",
                    "properties": {
                        "max": {
                            "type": "number",
                            "pattern": "^5$",
                            "required": "1"
                        },
                        "applies_to": {
                            "type": "string",
                            "required": "1"
                        }
                    }
                },
                "max_requests_pricing": {
                    "type": "object",
                    "required": "1",
                    "properties": {
                        "minutely": {
                            "type": "number",
                            "pattern": "^1000000$",
                            "required": "1"
                        },
                        "hourly": {
                            "type": "number",
                            "pattern": "^2000000$",
                            "required": "1"
                        },
                        "applies_to": {
                            "type": "string",
                            "required": "1"
                        }
                    }
                },
                "max_requests_outcome": {
                    "type": "object",
                    "required": "1",
                    "properties": {
                        "minutely": {
                            "type": "number",
                            "pattern": "^1000000$",
                            "required": "1"
                        },
                        "hourly": {
                            "type": "number",
                            "pattern": "^2000000$",
                            "required": "1"
                        },
                        "applies_to": {
                            "type": "string",
                            "required": "1"
                        }
                    }
                },
                "max_requestes_general": {
                    "type": "object",
                    "required": "1",
                    "properties": {
                        "minutely": {
                            "type": "number",
                            "pattern": "^1000000$",
                            "required": "1"
                        },
                        "hourly": {
                            "type": "number",
                            "pattern": "^2000000$",
                            "required": "1"
                        },
                        "applies_to": {
                            "type": "string",
                            "required": "1"
                        }
                    }
                }
            }
        }
    }
}
