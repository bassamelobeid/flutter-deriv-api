{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "stash",
      "country",
      "country_code",
      "email_consent",
      "email",
      "residence",
      "immutable_fields",
      "user_hash",
      "preferred_language"
   ],
   "title" : null,
   "additionalProperties" : null,
   "description" : null,
   "type" : "object",
   "properties" : {
      "residence" : {
         "type" : "string",
         "pattern" : "^[_1]$"
      },
      "country" : {
         "pattern" : "^[_1]$",
         "type" : "string"
      },
      "user_hash" : {
         "type" : "string"
      },
      "email" : {
         "type" : "string"
      },
      "country_code" : {
         "type" : "string",
         "pattern" : "^[_2]$"
      },
      "stash" : {
         "type" : "object"
      },
      "email_consent" : {
         "type" : "integer",
         "enum" : [
            0,
            1
         ]
      },
      "immutable_fields" : {
         "type" : "array",
         "items" : {
            "type" : "string"
         }
      },
      "preferred_language" : {
         "type" : "string",
         "pattern": "^[A-Z]{0,2}$|^[A-Z]{2}_[A-Z]{2}$"
      }
   }
}
