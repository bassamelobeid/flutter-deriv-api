{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "stash",
      "country",
      "country_code",
      "email_consent",
      "email",
      "residence",
      "immutable_fields",
      "user_hash",
      "trading_hub",
      "preferred_language"
   ],
   "title" : null,
   "additionalProperties" : null,
   "description" : null,
   "type" : "object",
   "properties" : {
      "residence" : {
         "type" : "string",
         "pattern" : "^[_1]$"
      },
      "country" : {
         "pattern" : "^[_1]$",
         "type" : "string"
      },
      "user_hash" : {
         "type" : "string"
      },
      "email" : {
         "type" : "string"
      },
      "feature_flag" : {
         "type" : "object",
         "properties": {
            "wallet": {
               "type": "integer",
               "enum": [
                  0,
                  1
               ]
            }
         }
      },
      "cooling_off_expiration_date" : {
         "type" : "integer"
      },
      "country_code" : {
         "type" : "string",
         "pattern" : "^[_2]$"
      },
      "stash" : {
         "type" : "object"
      },
      "email_consent" : {
         "type" : "integer",
         "enum" : [
            0,
            1
         ]
      },
      "immutable_fields" : {
         "type" : "array",
         "items" : {
            "type" : "string"
         }
      },
      "preferred_language" : {
         "type" : "string",
         "pattern": "^[A-Z]{0,2}$|^[A-Z]{2}_[A-Z]{2}$"
      },
      "trading_hub" : {
         "type" : "integer",
         "enum" : [
            0,
            1
         ]
      },
      "phone_number_verification": {
         "type": "object",
         "properties" : {
            "verified" : {
               "type" : "integer",
               "enum" : [
                  0,
                  1
               ]
            },
            "next_attempt" : {
               "type" : "integer"
            }
         }
      }
   }
}
