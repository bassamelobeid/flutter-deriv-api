{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "client_tnc_status",
      "is_authenticated_payment_agent",
      "stash",
      "salutation",
      "last_name",
      "email_consent",
      "email",
      "address_line_2",
      "address_postcode",
      "address_line_1",
      "phone",
      "country_code",
      "address_state",
      "address_city",
      "account_opening_reason",
      "country",
      "date_of_birth",
      "first_name",
      "non_pep_declaration",
      "fatca_declaration",
      "preferred_language",
      "trading_hub",
      "phone_number_verification"
   ],
   "description" : null,
   "type" : "object",
   "additionalProperties" : null,
   "title" : null,
   "properties" : {
      "address_line_2" : {
         "type" : "string",
         "pattern" : "^East Melbourne VIC$"
      },
      "client_tnc_status" : {
         "pattern" : "^Version \\d{2} \\d{4}-\\d{2}-\\d{2}$",
         "type" : "string"
      },
      "country_code" : {
         "type" : "string",
         "pattern" : "^id$"
      },
      "last_name" : {
         "type" : "string",
         "pattern" : "^Pan$"
      },
      "email_consent" : {
         "type" : "integer",
         "enum" : [
            0,
            1
         ]
      },
      "address_state" : {
         "pattern" : "^BT$",
         "type" : "string"
      },
      "salutation" : {
         "type" : "string",
         "pattern" : "^Mr$"
      },
      "first_name" : {
         "pattern" : "^Peter$",
         "type" : "string"
      },
      "country" : {
         "pattern" : "^Indonesia$",
         "type" : "string"
      },
      "address_postcode" : {
         "type" : "string",
         "pattern" : "^3002$"
      },
      "email" : {
         "type" : "string",
         "pattern" : "test@binary.com"
      },
      "phone" : {
         "pattern" : "^\\+6123456789$",
         "type" : "string"
      },
      "address_city" : {
         "type" : "string",
         "pattern" : "^Melbourne$"
      },
      "address_line_1" : {
         "pattern" : "^20 Broadway Av$",
         "type" : "string"
      },
      "stash" : {
         "type" : "object"
      },
      "is_authenticated_payment_agent" : {
         "enum" : [
            0
         ],
         "type" : "integer"
      },
      "account_opening_reason" : {
         "pattern" : "^Hedging$",
         "type" : "string"
      },
      "date_of_birth" : {
         "enum" : [
            318124800
         ],
         "type" : "integer"
      },
      "non_pep_declaration": {
        "description": "Indicates client's self-declaration of not being a PEP/RCA (Politically Exposed Person/Relatives and Close Associates).",
        "type": "integer",
        "enum": [
            1
        ]
      },
      "fatca_declaration": {
         "description": "Indicates client's self-declaration of FATCA.",
         "type": "integer",
         "enum": [
            0,
            1
         ]
      },
      "immutable_fields" : {
         "type" : "array",
         "items" : {
            "type" : "string"
         }
      },
      "preferred_language" : {
         "type" : "string",
         "pattern": "^[A-Z]{0,2}$|^[A-Z]{2}_[A-Z]{2}$"
      },
      "trading_hub" : {
         "type" : "integer",
         "enum" : [
            0,
            1
         ]
      },
      "phone_number_verification": {
         "type": "object",
         "properties" : {
            "verified" : {
               "type" : "integer",
               "enum" : [
                  0,
                  1
               ]
            },
            "next_attempt" : {
               "type" : "integer"
            }
         }
      }
   }
}
