{
    "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
            "balance_after": {
                "type": "number",
                "pattern": "^[_2]$",
                "required": 1
            },
            "longcode": {
                "type": "string",
                "required": 1
            },
            "shortcode": {
                "type": "string",
                "required": 1
            },
            "start_time": {
                "type": "integer",
                "pattern": "^\\d{10}$",
                "required": 1
            },
            "contract_id": {
                "type": "integer",
                "required": 1
            },
            "buy_price": {
                "type": "number",
                "required": 1
            },
            "purchase_time": {
                "type": "integer",
                "pattern": "^\\d{10}$",
                "required": 1
            },
            "transaction_id": {
                "type": "integer",
                "required": 1
            },
            "payout": {
                "type": "number",
                "pattern": "^[.\\d]+$",
                "required": 1
            }
        }
}

