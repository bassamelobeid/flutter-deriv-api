{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "title" : null,
   "additionalProperties" : false,
   "type" : "object",
   "description" : null,
   "required" : [
      "balance_after",
      "payout",
      "stash",
      "contract_id",
      "start_time",
      "transaction_id",
      "shortcode",
      "buy_price",
      "purchase_time",
      "longcode",
      "contract_details"
   ],
   "properties" : {
      "contract_details" : {
         "additionalProperties" : false,
         "type" : "object",
         "required" : [
            "transaction_ids",
            "is_sold",
            "sell_time",
            "longcode",
            "account_id",
            "buy_price",
            "shortcode",
            "contract_id",
            "sell_price",
            "currency",
            "purchase_time"
         ],
         "properties" : {
            "transaction_ids" : {
               "type" : "object",
               "additionalProperties" : "false",
               "required" : [
                  "buy"
               ],
               "properties" : {
                  "buy" : {
                     "pattern" : "^[0-9]+$",
                     "type" : "string"
                  }
               }
            },
            "is_sold" : {
               "enum" : [
                  0,
                  1
               ],
               "type" : "integer"
            },
            "sell_time" : {
               "maximum" : 9999999999,
               "minimum" : 1000000000,
               "type" : [
                  "integer",
                  "null"
               ]
            },
            "longcode" : {
               "type" : "string"
            },
            "account_id" : {
               "type" : "string",
               "pattern" : "^[0-9]+$"
            },
            "buy_price" : {
               "type" : "string",
               "pattern" : "^[0-9]+.[0.9]+$"
            },
            "shortcode" : {
               "type" : "string"
            },
            "contract_id" : {
               "pattern" : "^[0-9]+$",
               "type" : "string"
            },
            "sell_price" : {
               "type" : [
                  "string",
                  "null"
               ],
               "pattern" : "^[0-9]+.[0.9]+$"
            },
            "currency" : {
               "type" : "string",
               "pattern" : "^[A-Z]{3}$"
            },
            "purchase_time" : {
               "minimum" : 1000000000,
               "type" : "integer",
               "maximum" : 9999999999
            }
         }
      },
      "purchase_time" : {
         "maximum" : 9999999999,
         "minimum" : 1000000000,
         "type" : "integer"
      },
      "shortcode" : {
         "type" : "string"
      },
      "balance_after" : {
         "pattern" : "^[_2]$",
         "type" : "string"
      },
      "contract_id" : {
         "type" : "string"
      },
      "buy_price" : {
         "type" : "string"
      },
      "start_time" : {
         "type" : "integer",
         "minimum" : 1000000000,
         "maximum" : 9999999999
      },
      "transaction_id" : {
         "type" : "string"
      },
      "payout" : {
         "type" : "number",
         "mimimum" : 0
      },
      "stash" : {
         "type" : "object"
      },
      "longcode" : {
         "type" : "string"
      },
      "auth_time" : {
        "type": "number"
      },
      "rpc_time" : {
        "type": "number"
      }
   }
}
