{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "type" : "object",
   "description" : null,
   "title" : null,
   "additionalProperties" : false,
   "required" : [
      "msg_type",
      "stash",
      "echo_req",
      "error"
   ],
   "properties" : {
      "error" : {
         "required" : [
            "message",
            "code"
         ],
         "properties" : {
            "message" : {
               "type" : "string"
            },
            "code" : {
               "pattern" : "^APITokenError$",
               "type" : "string"
            }
         },
         "type" : "object",
         "additionalProperties" : false
      },
      "stash" : {
         "type" : "object"
      },
      "msg_type" : {
         "type" : "string",
         "pattern" : "^api_token$"
      },
      "echo_req" : {
         "properties" : {},
         "additionalProperties" : false,
         "type" : "object"
      }
   }
}
