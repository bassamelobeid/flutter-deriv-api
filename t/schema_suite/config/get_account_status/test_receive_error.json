{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "stash",
      "error"
   ],
   "additionalProperties" : false,
   "title" : null,
   "type" : "object",
   "description" : null,
   "properties" : {
      "stash" : {
         "type" : "object"
      },
      "error" : {
         "type" : "object",
         "additionalProperties" : false,
         "properties" : {
            "code" : {
               "pattern" : "^AuthorizationRequired$",
               "type" : "string"
            },
            "message" : {
               "type" : "string"
            }
         },
         "required" : [
            "message",
            "code"
         ]
      }
   }
}
