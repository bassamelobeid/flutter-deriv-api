{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^br$",
            "required": "1"
        },
        "name": {
            "type": "string",
            "pattern": "^Brazil$",
            "required": "1"
        },
        "virtual_company": {
            "type": "string",
            "pattern": "^virtual$",
            "required": "1"
        },
        "gaming_company": {
            "type": "object",
            "required": "1",
            "properties": {
                "shortcode": {
                    "type": "string",
                    "pattern": "^costarica$",
                    "required": "1"
                },
                "name": {
                    "type": "string",
                    "pattern": "^Binary \\(C\\.R\\.\\) S\\.A\\.$",
                    "required": "1"
                },
                "address": {
                    "type": "null",
                    "items": {
                        "type": "string"
                    }
                },
                "country": {
                    "type": "string",
                    "pattern": "^Costa Rica$",
                    "required": "1"
                },
                "legal_default_currency": {
                    "type": "string",
                    "pattern": "^USD$",
                    "required": "1"
                },
                "legal_allowed_currencies": {
                    "type": "array",
                    "minItems": 4,
                    "required": "1",
                    "items": {
                        "type": "string",
                        "pattern": "^(USD|EUR|GBP|AUD|BTC|LTC|BCH|ETH|UST|USB)$",
                        "required": "1"
                    }
                },
                "legal_allowed_markets": {
                    "type": "array",
                    "minItems": 4,
                    "required": "1",
                    "items": {
                        "type": "string",
                        "pattern": "^(commodities|forex|indices|volidx)$",
                        "required": "1"
                    }
                },
                "legal_allowed_contract_categories": {
                    "type": "array",
                    "minItems": 6,
                    "required": "1",
                    "items": {
                        "type": "string",
                        "pattern": "^(asian|callput|digits|endsinout|staysinout|touchnotouch|lookback|highlowticks|reset|callputspread|callputequal|runs)$",
                        "required": "1"
                    }
                },
                "has_reality_check": {
                    "type": "integer",
                    "pattern": "^0$",
                    "required": "1"
                }
            }
        },
        "financial_company": {
            "type": "object",
            "required": "1",
            "properties": {
                "shortcode": {
                    "type": "string",
                    "pattern": "^costarica$",
                    "required": "1"
                },
                "name": {
                    "type": "string",
                    "pattern": "^Binary \\(C\\.R\\.\\) S\\.A\\.$",
                    "required": "1"
                },
                "address": {
                    "type": "null",
                    "items": {
                        "type": "string"
                    }
                },
                "country": {
                    "type": "string",
                    "pattern": "^Costa Rica$",
                    "required": "1"
                },
                "legal_default_currency": {
                    "type": "string",
                    "pattern": "^USD$",
                    "required": "1"
                },
                "legal_allowed_currencies": {
                    "type": "array",
                    "minItems": 4,
                    "required": "1",
                    "items": {
                        "type": "string",
                        "pattern": "^(USD|EUR|GBP|AUD|BTC|LTC|BCH|ETH|UST|USB)$",
                        "required": "1"
                    }
                },
                "legal_allowed_markets": {
                    "type": "array",
                    "minItems": 4,
                    "required": "1",
                    "items": {
                        "type": "string",
                        "pattern": "^(commodities|forex|indices|volidx)$",
                        "required": "1"
                    }
                },
                "legal_allowed_contract_categories": {
                    "type": "array",
                    "minItems": 6,
                    "required": "1",
                    "items": {
                        "type": "string",
                        "pattern": "^(asian|callput|digits|endsinout|staysinout|touchnotouch|lookback|highlowticks|reset|callputspread|callputequal|runs)$",
                        "required": "1"
                    }
                },
                "has_reality_check": {
                    "type": "integer",
                    "pattern": "^0$",
                    "required": "1"
                }
            }
        }
    }
}
