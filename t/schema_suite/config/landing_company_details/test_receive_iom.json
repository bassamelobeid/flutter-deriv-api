{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "type" : "object",
   "description" : null,
   "title" : null,
   "additionalProperties" : false,
   "required" : [
      "legal_default_currency",
      "shortcode",
      "address",
      "legal_allowed_markets",
      "legal_allowed_currencies",
      "country",
      "stash",
      "has_reality_check",
      "name",
      "legal_allowed_contract_categories",
      "changeable_fields",
      "currency_config",
      "requirements"
   ],
   "properties" : {
      "address" : {
         "items" : {
            "type" : "string"
         },
         "type" : "array"
      },
      "name" : {
         "type" : "string",
         "pattern" : "^Deriv \\(MX\\) Ltd$"
      },
      "shortcode" : {
         "pattern" : "^[_1]$",
         "type" : "string"
      },
      "legal_default_currency" : {
         "type" : "string",
         "pattern" : "^GBP$"
      },
      "legal_allowed_currencies" : {
         "type" : "array",
         "items" : {
            "pattern" : "^(USD|GBP)$",
            "type" : "string"
         },
         "minItems" : 2
      },
      "legal_allowed_markets" : {
         "items" : {
            "type" : "string",
            "pattern" : "^(commodities|forex|indices|synthetic_index)$"
         },
         "type" : "array",
         "minItems" : 4
      },
      "country" : {
         "type" : "string",
         "pattern" : "^Isle of Man$"
      },
      "currency_config" : {
         "additionalProperties" : false,
         "type" : "object",
         "properties" : {
            "indices" : {
               "additionalProperties" : false,
               "type" : "object",
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "min_stake" : {
                           "minimum" : 0,
                           "type" : "number"
                        },
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "additionalProperties" : false,
                     "type" : "object"
                  }
               },
               "properties" : {}
            },
            "synthetic_index" : {
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "properties" : {
                        "max_payout" : {
                           "minimum" : 0,
                           "type" : "number"
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0,
                           "maximum" : 10
                        }
                     },
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "type" : "object",
                     "additionalProperties" : false
                  }
               },
               "properties" : {},
               "additionalProperties" : false,
               "type" : "object"
            },
            "commodities" : {
               "type" : "object",
               "additionalProperties" : false,
               "properties" : {},
               "patternProperties" : {
                  "^[A-Z]{3,4}$" : {
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "additionalProperties" : false,
                     "type" : "object"
                  }
               }
            },
            "forex" : {
               "additionalProperties" : false,
               "type" : "object",
               "patternProperties" : {
                  "^[A-Z]{3,4}$" : {
                     "type" : "object",
                     "additionalProperties" : false,
                     "properties" : {
                        "min_stake" : {
                           "minimum" : 0,
                           "type" : "number"
                        },
                        "max_payout" : {
                           "minimum" : 0,
                           "type" : "number"
                        }
                     },
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ]
                  }
               },
               "properties" : {}
            }
         },
         "required" : [
            "indices",
            "synthetic_index",
            "commodities",
            "forex"
         ]
      },
      "stash" : {
         "type" : "object"
      },
      "has_reality_check" : {
         "type" : "integer",
         "enum" : [
            1
         ]
      },
      "changeable_fields" : {
         "additionalProperties" : false,
         "type" : "object",
         "properties" : {}
      },
      "requirements" : {
         "required" : [
            "signup"
         ],
         "properties" : {
            "signup" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            }
         },
         "type" : "object",
         "additionalProperties" : false
      },
      "legal_allowed_contract_categories" : {
         "type" : "array",
         "items" : {
            "pattern" : "^(asian|callput|digits|endsinout|staysinout|touchnotouch|callputequal|callputspread|reset|highlowticks|lookback|runs|multiplier)$",
            "type" : "string"
         },
         "minItems" : 6
      }
   }
}
