{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "stash",
      "country",
      "legal_allowed_currencies",
      "legal_allowed_markets",
      "legal_allowed_contract_categories",
      "name",
      "has_reality_check",
      "legal_default_currency",
      "address",
      "shortcode",
      "changeable_fields",
      "currency_config",
      "requirements"
   ],
   "description" : null,
   "type" : "object",
   "additionalProperties" : false,
   "title" : null,
   "properties" : {
      "stash" : {
         "type" : "object"
      },
      "has_reality_check" : {
         "type" : "integer",
         "enum" : [
            1
         ]
      },
      "changeable_fields" : {
         "properties" : {},
         "additionalProperties" : false,
         "type" : "object"
      },
      "requirements" : {
         "type" : "object",
         "additionalProperties" : false,
         "required" : [
            "signup"
         ],
         "properties" : {
            "signup" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            }
         }
      },
      "legal_allowed_contract_categories" : {
         "items" : {
            "pattern" : "^(asian|callput|digits|endsinout|staysinout|touchnotouch|callputequal|callputspread|reset|highlowticks|lookback|runs)$",
            "type" : "string"
         },
         "type" : "array",
         "minItems" : 6
      },
      "name" : {
         "type" : "string",
         "pattern" : "^Deriv \\(Europe\\) Limited$"
      },
      "address" : {
         "items" : {
            "type" : "string"
         },
         "type" : "array"
      },
      "shortcode" : {
         "type" : "string",
         "pattern" : "^[_1]$"
      },
      "legal_default_currency" : {
         "type" : "string",
         "pattern" : "^EUR$"
      },
      "legal_allowed_currencies" : {
         "minItems" : 3,
         "type" : "array",
         "items" : {
            "type" : "string",
            "pattern" : "^(USD|EUR|GBP)$"
         }
      },
      "legal_allowed_markets" : {
         "type" : "array",
         "items" : {
            "pattern" : "^(synthetic_index)$",
            "type" : "string"
         },
         "minItems" : 1
      },
      "country" : {
         "pattern" : "^Malta$",
         "type" : "string"
      },
      "currency_config" : {
         "type" : "object",
         "additionalProperties" : false,
         "required" : [
            "synthetic_index"
         ],
         "properties" : {
            "synthetic_index" : {
               "additionalProperties" : false,
               "type" : "object",
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "maximum" : 10,
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "additionalProperties" : false,
                     "type" : "object"
                  }
               },
               "properties" : {}
            }
         }
      }
   }
}
