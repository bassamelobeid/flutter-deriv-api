{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "shortcode": {
            "type": "string",
            "pattern": "^[_1]$",
            "required": "1"
        },
        "name": {
            "type": "string",
            "pattern": "^Binary Virtual Japan$",
            "required": "1"
        },
        "address": {
            "type": "null"
        },
        "country": {
            "type": "string",
            "pattern": "^Japan$",
            "required": "1"
        },
        "legal_default_currency": {
            "type": "string",
            "pattern": "^JPY$",
            "required": "1"
        },
        "legal_allowed_currencies": {
            "type": "array",
            "minItems": 1,
            "required": "1",
            "items": {
                "type": "string",
                "pattern": "^JPY$",
                "required": "1"
            }
        },
        "legal_allowed_markets": {
            "type": "array",
            "minItems": 1,
            "required": "1",
            "items": {
                "type": "string",
                "pattern": "^forex$",
                "pattern": "^forex$",
                "required": "1"
            }
        },
        "legal_allowed_contract_categories": {
            "type": "array",
            "minItems": 4,
            "required": "1",
            "items": {
                "type": "string",
                "pattern": "^(callput|endsinout|staysinout|touchnotouch|callputequal)$",
                "required": "1"
            }
        },
        "has_reality_check": {
            "type": "integer",
            "pattern": "^0$",
            "required": "1"
        }
    }
}
