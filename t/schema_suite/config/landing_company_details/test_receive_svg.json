{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "title" : null,
   "additionalProperties" : false,
   "type" : "object",
   "description" : null,
   "required" : [
      "shortcode",
      "legal_default_currency",
      "legal_allowed_contract_categories",
      "has_reality_check",
      "name",
      "legal_allowed_currencies",
      "country",
      "stash",
      "legal_allowed_markets",
      "changeable_fields",
      "currency_config",
      "requirements",
      "address"
   ],
   "properties" : {
      "name" : {
         "type" : "string",
         "pattern" : "^Deriv \\(SVG\\) LLC$"
      },
      "address" : {
         "type" : "null"
      },
      "legal_default_currency" : {
         "pattern" : "^USD$",
         "type" : "string"
      },
      "shortcode" : {
         "type" : "string",
         "pattern" : "^[_1]$"
      },
      "currency_config" : {
         "additionalProperties" : false,
         "type" : "object",
         "properties" : {
            "cryptocurrency" : {
               "additionalProperties" : false,
               "type" : "object",
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "min_stake" : {
                           "minimum" : 0,
                           "type" : "number"
                        },
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "additionalProperties" : false,
                     "type" : "object"
                  }
               },
               "properties" : {}
            },
            "indices" : {
               "additionalProperties" : false,
               "type" : "object",
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "min_stake" : {
                           "minimum" : 0,
                           "type" : "number"
                        },
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "additionalProperties" : false,
                     "type" : "object"
                  }
               },
               "properties" : {}
            },
            "synthetic_index" : {
               "properties" : {},
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "additionalProperties" : false,
                     "type" : "object",
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ]
                  }
               },
               "type" : "object",
               "additionalProperties" : false
            },
            "commodities" : {
               "type" : "object",
               "additionalProperties" : false,
               "properties" : {},
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0
                        }
                     },
                     "additionalProperties" : false,
                     "type" : "object"
                  }
               }
            },
            "forex" : {
               "additionalProperties" : false,
               "type" : "object",
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "type" : "object",
                     "additionalProperties" : false,
                     "properties" : {
                        "min_stake" : {
                           "minimum" : 0,
                           "type" : "number"
                        },
                        "max_payout" : {
                           "minimum" : 0,
                           "type" : "number"
                        }
                     },
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ]
                  }
               },
               "properties" : {}
            }
         },
         "required" : [
            "indices",
            "synthetic_index",
            "commodities",
            "forex",
            "cryptocurrency"
         ]
      },
      "country" : {
         "type" : "string",
         "pattern" : "^Saint Vincent and the Grenadines$"
      },
      "legal_allowed_currencies" : {
         "type" : "array",
         "items" : {
            "pattern" : "^[a-zA-Z0-9]{2,20}$",
            "type" : "string"
         },
         "minItems" : 4
      },
      "legal_allowed_markets" : {
         "items" : {
            "type" : "string",
            "pattern" : "^(commodities|forex|indices|synthetic_index|cryptocurrency)$"
         },
         "type" : "array",
         "minItems" : 4
      },
      "has_reality_check" : {
         "enum" : [
            0,
            1
         ],
         "type" : "integer"
      },
      "stash" : {
         "type" : "object"
      },
      "changeable_fields" : {
         "additionalProperties" : false,
         "type" : "object",
         "patternProperties" : {
            "^.+$" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            }
         },
         "properties" : {}
      },
      "requirements" : {
         "properties" : {},
         "patternProperties" : {
            "^.+$" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            }
         },
         "type" : "object",
         "additionalProperties" : false
      },
      "legal_allowed_contract_categories" : {
         "minItems" : 6,
         "type" : "array",
         "items" : {
            "type" : "string",
            "pattern" : "^(asian|callput|digits|endsinout|staysinout|touchnotouch|lookback|highlowticks|reset|callputspread|callputequal|runs|multiplier)$"
         }
      }
   }
}
