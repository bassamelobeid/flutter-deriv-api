{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "score": {
      "type": "integer",
      "pattern": "^[_2]$",
      "required": "1"
    },
    "forex_trading_frequency": {
      "type": "string",
      "pattern": "^40 transactions or more in the past 12 months$",
      "required": "1"
    },
    "forex_trading_experience": {
      "type": "string",
      "pattern": "^1\\-2 years$",
      "required": "1"
    },
    "binary_option_trading_frequency": {
      "type": "string",
      "pattern": "^0\\-5 transactions in the past 12 months$",
      "required": "1"
    },
    "binary_option_trading_experience": {
      "type": "string",
      "pattern": "^Over 3 years$",
      "required": "1"
    },
    "cfd_trading_frequency": {
      "type": "string",
      "pattern": "^40 transactions or more in the past 12 months$",
      "required": "1"
    },
    "cfd_trading_experience": {
      "type": "string",
      "pattern": "^Over 3 years$",
      "required": "1"
    },
    "other_instruments_trading_frequency": {
      "type": "string",
      "pattern": "^40 transactions or more in the past 12 months$",
      "required": "1"
    },
    "other_instruments_trading_experience": {
      "type": "string",
      "pattern": "^1\\-2 years$",
      "required": "1"
    },
    "employment_industry": {
      "type": "string",
      "pattern": "^Health$",
      "required": "1"
    },
    "education_level": {
      "type": "string",
      "pattern": "^Tertiary$",
      "required": "1"
    },
    "income_source": {
      "type": "string",
      "pattern": "^Investments \\& Dividends$",
      "required": "1"
    },
    "net_income": {
      "type": "string",
      "pattern": "^Over \\$500\\,000$",
      "required": "1"
    },
    "estimated_worth": {
      "type": "string",
      "pattern": "^Less than \\$100\\,000$",
      "required": "1"
    },
    "occupation": {
      "type": "string",
      "pattern": "^Managers$",
      "required": 1
    },
    "employment_status": {
      "type": "string",
      "pattern": "^Self-Employed$",
      "required": 0
    },
    "source_of_wealth": {
      "type": "string",
      "pattern": "^Company Ownership$",
      "required": 0
    }
  }
}
