{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "pattern": "^[_3]$",
      "required": "1"
    },
    "currency": {
      "type": "string",
      "pattern": "^$",
      "required": "1"
    },
    "balance": {
      "type": "number",
      "pattern": "^0.00$",
      "required": "1"
    },
    "loginid": {
      "type": "string",
      "pattern": "^CR\\d+$",
      "required": "1"
    },
    "is_virtual": {
      "type": "string",
      "pattern": "^0$",
      "required": "1"
    },
    "landing_company_name": {
      "type": "string",
      "pattern": "^svg$",
      "required": "1"
    },
    "landing_company_fullname": {
      "type": "string",
      "pattern": "^Binary \\(SVG\\) Ltd\\.$",
      "required": "1"
    },
    "country": {
      "type": "string",
      "pattern": "^[_2]$",
      "required": "1"
    },
    "fullname": {
      "type": "string",
      "pattern": "^Mrs Example First Name Example Last Name$",
      "required": "1"
    },
    "scopes": {
      "type": "array",
      "minItems": 4,
      "items": {
        "type": "string",
        "pattern": "^(admin|payments|read|trade)$"
      },
      "required": "1"
    }
  }
}
