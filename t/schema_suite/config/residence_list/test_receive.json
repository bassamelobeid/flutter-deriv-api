{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "required": "1",
    "items": {
        "type": "object",
        "properties": {
            "phone_idd": {
                "type": "string",
                "pattern": "^\\d+$"
            },
            "text": {
                "type": "string",
                "required": "1"
            },
            "value": {
                "type": "string",
                "pattern": "^[a-z]{2}$",
                "required": "1"
            }
        }
    }
}
