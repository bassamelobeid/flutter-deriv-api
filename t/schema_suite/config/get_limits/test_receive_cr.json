               "properties" : {
                  "allowed" : {
                     "type" : "integer"
                  },
                  "available" : {
                     "type" : "integer"
                  }
               }
            },
            "internal" : {
               "type" : "object",
               "additionalProperties" : false,
               "required" : [
                  "allowed",
                  "available"
               ],
               "properties" : {
                  "allowed" : {
                     "type" : "integer"
                  },
                  "available" : {
                     "type" : "integer"
                  }
