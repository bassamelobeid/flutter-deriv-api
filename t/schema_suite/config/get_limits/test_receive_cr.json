            "indices" : {
               "type" : "array",
               "items" : {
                  "required" : [
                     "name",
                     "turnover_limit"
                  ],
                  "properties" : {
                     "name" : {
                        "pattern" : "^Indices$",
                        "type" : "string"
                     },
                     "turnover_limit" : {
                        "enum" : [
                           100000
                        ],
                        "type" : "integer"
                     }
                  },
                  "additionalProperties" : false,
                  "type" : "object"
               }
            },
            "commodities" : {
               "items" : {
                  "properties" : {
                     "turnover_limit" : {
                        "enum" : [
                           50000
                        ],
                        "type" : "integer"
                     },
                     "name" : {
                        "type" : "string",
                        "pattern" : "^Commodities$"
                     }
                  },
                  "required" : [
                     "turnover_limit",
                     "name"
                  ],
                  "type" : "object",
                  "additionalProperties" : false
               },
               "type" : "array"
            },
            "synthetic_index" : {
