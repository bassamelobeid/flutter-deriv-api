      "account_balance",
      "num_of_days_limit",
      "remainder",
      "withdrawal_since_inception_monetary",
      "lifetime_limit",
      "stash",
      "payout",
      "num_of_days",
      "open_positions",
      "withdrawal_for_x_days_monetary",
      "market_specific",
      "daily_transfers"
