      "account_balance" : {
         "enum" : [
            0,
            300000
         ],
         "type" : "integer"
      },
      "num_of_days_limit" : {
         "type" : "integer",
         "enum" : [
            10000
         ]
      },
      "withdrawal_since_inception_monetary" : {
