                  "required" : [
                     "turnover_limit",
                     "name"
                  ],
                  "properties" : {
                     "turnover_limit" : {
                        "type" : "integer",
                        "enum" : [
                           100000,
                           50000
                        ]
                     },
                     "name" : {
                        "pattern" : "^(Major Pairs|Minor Pairs)$",
                        "type" : "string"
                     }
                  },
                  "type" : "object",
                  "additionalProperties" : false
               }
            },
            "basket_index" : {
               "type" : "array",
               "items" : {
                  "required" : [
                     "turnover_limit",
                     "name"
                  ],
                  "properties" : {
                     "turnover_limit" : {
                        "type" : "integer",
                        "enum" : [
                           100000,
                           50000
                        ]
                     },
                     "name" : { 
                        "pattern" : "^(Commodities|Forex) Basket$",
                        "type" : "string"
                     }
                  },
                  "type" : "object",
                  "additionalProperties" : false
               }
            }
         },
         "type" : "object",
         "additionalProperties" : false
