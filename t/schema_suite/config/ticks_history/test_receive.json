{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
       "stash": {
           "type": "object",
           "required": "1",
           "properties":{
               "[_1]_display_decimals": {
                   "type": "integer",
                   "required": "1"
               }
           }
       },
       "type":{
           "type": "string",
           "required": "1",
           "pattern": "^history$"
       },
       "publish": {
           "type": "string",
           "required": "1",
           "pattern": "^tick$"
       },
       "data": {
           "type": "object",
           "required": "1",
           "properties": {
               "times": {
                   "type": ["array", "null"],
                   "required": "1",
                   "maxItems": "[_2]",
                   "minItems": "[_2]",
                   "items": {
                       "type": "integer",
                       "pattern": "^\\d{10}$",
                       "required": "1"
                   }
               },
               "prices": {
                   "type": ["array", "null"],
                   "required": "1",
                   "maxItems": "[_2]",
                   "minItems": "[_2]",
                   "items": {
                       "type": "string",
                       "pattern": "^\\d+\\.\\d+$",
                       "required": "1"
                   }
               }
           }
       }
        
    }
}
