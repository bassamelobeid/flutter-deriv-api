{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "legal_default_currency",
      "shortcode",
      "address",
      "legal_allowed_markets",
      "country",
      "stash",
      "legal_allowed_currencies",
      "name",
      "has_reality_check",
      "legal_allowed_contract_categories",
      "changeable_fields",
      "currency_config",
      "requirements",
      "support_professional_client"
   ],
   "additionalProperties" : false,
   "title" : null,
   "type" : "object",
   "description" : null,
   "properties" : {
      "currency_config" : {
         "properties" : {
            "forex" : {
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "additionalProperties" : false,
                     "type" : "object",
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0,
                           "maximum" : 10
                        }
                     }
                  }
               },
               "properties" : {},
               "additionalProperties" : false,
               "type" : "object"
            },
            "cryptocurrency" : {
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "additionalProperties" : false,
                     "type" : "object",
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0,
                           "maximum" : 10
                        }
                     }
                  }
               },
               "properties" : {},
               "additionalProperties" : false,
               "type" : "object"
            },
            "synthetic_index" : {
               "patternProperties" : {
                  "^[a-zA-Z0-9]{2,20}$" : {
                     "additionalProperties" : false,
                     "type" : "object",
                     "required" : [
                        "max_payout",
                        "min_stake"
                     ],
                     "properties" : {
                        "max_payout" : {
                           "type" : "number",
                           "minimum" : 0
                        },
                        "min_stake" : {
                           "type" : "number",
                           "minimum" : 0,
                           "maximum" : 10
                        }
                     }
                  }
               },
               "properties" : {},
               "additionalProperties" : false,
               "type" : "object"
            }
         },
         "required" : [
            "forex"
         ],
         "type" : "object",
         "additionalProperties" : false
      },
      "tin_not_mandatory": {
         "description": "Flag that indicates whether tax identifier number is not mandatory for the current country and landing company.",
         "type": "integer",
         "enum": [
             0,
             1
         ]
      },
      "country" : {
         "type" : "string",
         "pattern" : "^Malta$"
      },
      "legal_allowed_currencies" : {
         "type" : "array",
         "items" : {
            "pattern" : "^(USD|EUR)$",
            "type" : "string"
         },
         "minItems" : 2
      },
      "legal_allowed_markets" : {
         "type" : "array",
         "items" : {
            "pattern" : "^(forex|synthetic_index|cryptocurrency)$",
            "type" : "string"
         },
         "minItems" : 1
      },
      "name" : {
         "pattern" : "^Deriv Investments \\(Europe\\) Limited$",
         "type" : "string"
      },
      "address" : {
         "type" : "array",
         "items" : {
            "type" : "string"
         }
      },
      "legal_default_currency" : {
         "type" : "string",
         "pattern" : "^EUR$"
      },
      "shortcode" : {
         "type" : "string",
         "pattern" : "^[_1]$"
      },
      "support_professional_client" : {
         "type" : "integer",
         "enum" : [
            1
         ]
      },
      "requirements" : {
         "type" : "object",
         "additionalProperties" : false,
         "required" : [
            "signup",
            "compliance"
         ],
         "properties" : {
            "signup" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            },
            "compliance" : {
               "type" : "object",
               "additionalProperties" : false,
               "required" : [
                  "mt5",
                  "tax_information"
               ],
               "properties": {
                  "mt5" : {
                     "type" : "array",
                     "items": {
                        "type" : "string"
                     }
                  },
                  "tax_information" : {
                     "type" : "array",
                     "items": {
                        "type" : "string"
                     }
                  }
               }
            }
         }
      },
      "legal_allowed_contract_categories" : {
         "items" : {
            "type" : "string",
            "pattern" : "^multiplier|turbos$"
         },
         "type" : "array",
         "minItems" : 1
      },
      "has_reality_check" : {
         "type" : "integer",
         "enum" : [
            0
         ]
      },
      "stash" : {
         "type" : "object"
      },
      "changeable_fields" : {
         "additionalProperties" : false,
         "type" : "object",
         "patternProperties" : {
            "^.+$" : {
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            }
         },
         "properties" : {}
      }
   }
}
