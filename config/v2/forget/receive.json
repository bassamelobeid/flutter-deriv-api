{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Cancel an Incoming Stream Receive",
  "properties": {
    "echo_req": {
      "title": "Echo request",
      "description": "Echo of the request made"
    },
    "forget": {
    	"type": "string",
    	"description": "Id of the stream to cancel",
    	"pattern": "[a-z0-9]{32}"
    		
    },
    "msg_type": {
        "type": "string",
        "description": "forget"
      }
  },
  "required": [
    "echo_req",
    "msg_type"
  ]
}
