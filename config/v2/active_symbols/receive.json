{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type": "object",
  "title": "Get list of active symbols Receive",
  "description": "A message list of active symbols",
  "properties": {
    "echo_req": {
      "title": "Echo request",
      "description": "Echo of the request made"
    },
    "active_symbols": {
      "type":"array",
      "items":{
        "type":"object",
        "properties":{
          "symbol": {
            "type": "string",
            "description": "Symbol"
          },
          "intraday_interval_minutes": {
            "type": "integer",
            "description": "Intraday interval minutes"
          },
          "symbol_type": {
            "type": "string",
            "description": "Symbol type"
          },
          "exchange_is_open": {
            "type": ["integer", "null"],
            "description": "1 or 0"
          },
          "exchange_name": {
            "type": "string",
            "description": "Exchange name"
          },
          "delay_amount": {
            "type": "integer",
            "description": "Amount the feed is delayed (in minutes)"
          },
          "display_name": {
            "type": "string",
            "description": "display name"
          },
          "spot": {
            "type": ["number", "null"],
            "description": "Spot"
          },
          "spot_time": {
            "type": "string",
            "description": "Epoch of spot time."
          },
          "is_trading_suspended": {
            "type": "integer",
            "description": "1 indicates suspended trade."
          },
          "quoted_currency_symbol": {
            "type": "string",
            "description": "Quoted currency symbol"
          },
          "spot_age": {
            "type": "string",
            "description": "Spot age"
          },
          "market_name": {
            "type": "string",
            "description": "Market name"
          },
          "market_display_name": {
            "type": "string",
            "description": "Translated market name"
          },
          "pip": {
            "type": "number",
            "description": "pip size"
          }
        }
      },
      "description":"active symbols"
    },
    "msg_type": {
      "type": "string",
      "description": "active_symbols"
    }
  },
  "required": [
    "echo_req",
    "active_symbols",
    "msg_type"
  ]
}
