{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title":"Offering discovery send",
  "description":"Discover offerings presently available",
  "properties": {
    "offerings": {
      "type": "string",
      "description": "This must be 1.",
      "sample":"1"
    },
    "symbol": {
      "type": "string",
      "description": "Specifies the symbol",
      "sample":"R_100"
    },
    "hierarchy": {
      "type": "string",
      "description": "Include a hierarchical tree structure in the results (default: 1)",
      "sample":"1"
    },
    "selectors": {
      "type": "string",
      "description": "Include a report of values selectors in the results (default: 1)",
      "sample":"1"
    },
    "contracts": {
      "type": "string",
      "description": "Include a flat list in the results (default: 0)",
      "sample":"1"
    }
  },

  "required": [
    "offerings"
  ]
}
