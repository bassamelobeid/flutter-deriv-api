{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Tick History Response",
  "description": "Historic tick data for a single symbol",
  "properties": {
    "echo_req": {
      "title": "Echo request",
      "description": "Echo of the request made"
    },
    	"history": {
    	      "type": "object",
    	      "properties": {
    	        "times": {
    	          "type": "array",
    	          "items": [
    	            {
    	              "type": "integer",
    	              "description": "Epoch of the tick."
    	            }
    	          ]
    	        },
    	        "prices": {
    	          "type": "array",
    	          "items": [
    	            {
    	              "type": "number",
    	              "description": "Tick value."
    	            }
    	          ]
    	        }
    	      }
    	    },
    	    "candles": {
    	    	"type":"array",
    	          "items":{
    	            "type":"object"
    	          },
    	          "minItems":"1",
    	          "description":"Array of OHLC price values for the given time"
    	    },
    
    "msg_type": {
      "type": "string",
      "description": "style"
    }
  },
  "required": [
    "echo_req",
    "msg_type"
  ]
}
