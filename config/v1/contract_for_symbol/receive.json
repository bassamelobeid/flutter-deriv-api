{
    "$schema": "http://json-schema.org/draft-04/schema#",
    
    "type": "object",
	"title":"Contract for symbol receive ",
	"description":"A message with available contracts for symbol ",
    "properties": {
        "echo_req": {
            "title":"Echo request",
      	    "description":"Echo of the request made"
        },
        "contracts_for": {
           
            "type": "object",
            "properties": {
                "open": {
                    
                    "type": "integer",
		    "description":"time in integer format e.g 1440115200"
                },
                "close": {
                    
                    "type": "integer",
		    "description":"time in integer format e.g 1440201599"
                },
                "hit_count": {
                    
                    "type": "integer",
		    "description": "hit count e.g. 30"
					
                },
                "available": {
                    
                    "type": "array",
                    "items": [
            			{
			              "type": "object",
			              "description": "contracts for specified symbol"
			         }
			      ]
                }
            }
        },
        "msg_type": {
            
            "type": "string",
			"description": "contracts_for"
        }
    },
    "required": [
        "echo_req",
        "contracts_for",
        "msg_type"
    ]
}
