{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "New Account (response)",
    "description": "Create account Receive",
    "type": "object",
    "required": [
        "echo_req",
        "msg_type"
    ],
    "properties": {
        "new_account": {
            "title": "new_account",
            "description": "New account details",
            "type": "object",
            "required": [
                "balance",
                "client_loginid",
                "currency",
                "email",
                "oauth_token"
            ],
            "properties": {
                "balance": {
                    "description": "Account balance",
                    "type": "number"
                },
                "client_loginid": {
                    "description": "Client login ID of the new account",
                    "type": "string"
                },
                "currency": {
                    "description": "Account currency",
                    "type": "string"
                },
                "email": {
                    "description": "Email of the new account",
                    "type": "string"
                },
                "oauth_token": {
                    "description": "Oauth token for the new client's login session.",
                    "type": "string"
                }
            }
        },
        "echo_req": {
            "description": "Echo of the request made.",
            "type": "object"
        },
        "msg_type": {
            "description": "Action name of the request made.",
            "type": "string",
            "enum": [
                "new_account"
            ]
        },
        "req_id": {
            "description": "Optional field sent in request to map to response, present only when request contains `req_id`.",
            "type": "integer"
        }
    }
}
