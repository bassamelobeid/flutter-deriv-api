{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
      "granularity": 60,
      "count": 10,
      "ticks_history": "R_50",
      "style": "ticks",
      "end": "latest"
    },
    "history": {
      "type": "object",
      "properties": {
        "times": {
          "type": "array",
          "items": [
            {
              "type": "integer",
              "pattern": "^\\d{10}$"
            }
          ]
        },
        "prices": {
          "type": "array",
          "items": [
            {
              "type": "string",
              "pattern": "^\\d+\\.\\d+$"
            }
          ]
        }
      }
    },
    "msg_type": {
      "type": "string"
    },
    "req_id": {
      "type": "integer"
    }
  },
  "required": [
    "echo_req",
    "msg_type"
  ]
}
