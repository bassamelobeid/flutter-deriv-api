{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
      "granularity": {
        "type": "number",
        "pattern": "^60$"
        },
      "count": {
        "type": "number",
        "pattern": "^10$"
      },
      "ticks_history": {
        "type": "string",
        "pattern": "^R_50$"
      },
      "style": {
        "type": "string",
        "pattern": "^ticks$"
      },
      "end": {
        "type": "string",
        "pattern": "^latest$"
      },
      "required": "1"
    },
    "history": {
      "type": "object",
      "required": "1",
      "properties": {
        "required": "1",
        "times": {
          "type": ["array","null"],
          "maxItems": 10,
          "minItems": 10,
          "required": "1",
          "items":
            {
              "type": "integer",
              "pattern": "^\\d{10}$",
              "required": "1"
            }
        },
        "prices": {
          "type": ["array","null"],
          "maxItems": 10,
          "minItems": 10,
          "required": "1",
          "items":
            {
              "type": "string",
              "pattern": "^\\d+\\.\\d+$",
              "required": "1"
            }
        }
      }
    },
    "msg_type": {
      "type": "string",
      "required": "1"
    }
  }
}
