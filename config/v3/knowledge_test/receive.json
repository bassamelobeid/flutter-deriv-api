{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Knowledge Test Receive",
  "description": "A message with Knowledge Test",
  "properties": {
    "echo_req": {
      "title":"Echo request",
      "description":"Echo of the request made"
    },
    "knowledge_test": {
      "type": "integer",
      "title": "Japan Account Knowledge Test",
      "description": "Update Status, 1 on success"
    },
    "msg_type": {
      "type": "string",
      "description": "knowledge_test"
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field send in request to map to response, present only when request contains req_id"
    }
  },
  "required": [
    "echo_req",
    "knowledge_test",
    "msg_type"
  ]
}
