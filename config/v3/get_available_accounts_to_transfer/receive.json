{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Get Available Accounts to Transfer (response)",
    "description": "Get Available Accounts to Transfer.",
    "type": "object",
    "required": [
        "echo_req",
        "msg_type"
    ],
    "properties": {
        "get_available_accounts_to_transfer": {
            "title": "get_available_accounts_to_transfer",
            "description": "Returns a list of accounts available to transfer",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "account_list"
            ],
            "properties": {
                "account_list": {
                    "description": "List of available wallets/trading accounts for transfer.",
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "required": [
                            "account_type",
                            "currency",
                            "loginid",
                            "type"
                        ],
                        "properties": {
                            "account_type": {
                                "description": "Account type.",
                                "type": "string",
                                "enum": [
                                    "trading",
                                    "wallet"
                                ]
                            },
                            "currency": {
                                "description": "Currency of specified account.",
                                "type": "string"
                            },
                            "loginid": {
                                "description": "The loginid of specified account.",
                                "type": "string"
                            },
                            "type": {
                                "description": "Wallet or trading account type.",
                                "type": "string",
                                "enum": [
                                    "fiat",
                                    "crypto",
                                    "p2p",
                                    "pa",
                                    "pa_client"
                                ]
                            }
                        }
                    }
                }
            }
        },
        "echo_req": {
            "description": "Echo of the request made.",
            "type": "object"
        },
        "msg_type": {
            "description": "Action name of the request made.",
            "type": "string",
            "enum": [
                "get_available_accounts_to_transfer"
            ]
        },
        "req_id": {
            "description": "Optional field sent in request to map to response, present only when request contains `req_id`.",
            "type": "integer"
        }
    }
}
