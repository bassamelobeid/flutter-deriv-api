{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Get Available Accounts to Transfer (request)",
    "description": "Get Available Accounts to Transfer.",
    "hidden": 1,
    "type": "object",
    "auth_required": 1,
    "auth_scopes": [
        "payments"
    ],
    "additionalProperties": false,
    "required": [
        "get_available_accounts_to_transfer",
        "loginid"
    ],
    "properties": {
        "get_available_accounts_to_transfer": {
            "description": "Must be `1`",
            "type": "number",
            "enum": [
                1
            ]
        },
        "loginid": {
            "description": "The unique identifier for this trading/wallet account.",
            "type": "string",
            "pattern": "^[A-Za-z]+[0-9]+$"
        },
        "passthrough": {
            "description": "[Optional] Used to pass data through the websocket, which may be retrieved via the `echo_req` output field.",
            "type": "object"
        },
        "req_id": {
            "description": "[Optional] Used to map request to response.",
            "type": "integer"
        }
    }
}
