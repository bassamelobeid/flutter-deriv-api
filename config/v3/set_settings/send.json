{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Set User Settings Send",
  "description":"Set User Settings",
  "properties": {
    "set_settings": {
      "type":"integer",
      "pattern": "^1$",
      "required": "1"
    },
    "address_line_1":{
      "type":"string",
      "pattern": "^.{1,70}$",
      "required": "1"
    },
    "address_line_2":{
      "type": ["string", "null"],
      "pattern": "^.{0,70}$"
    },
    "address_city":{
      "type":"string",
      "pattern": "^.{1,35}$",
      "required": "1"
    },
    "address_state":{
      "type":"string",
      "pattern": "^.{1,35}$",
      "required": "1"
    },
    "address_postcode":{
      "type":"string",
      "pattern": "^[\\w\\s-]{1,20}$",
      "required": "1"
    },
    "phone":{
      "type": ["string", "null"],
      "pattern": "^\\+?[0-9\\s]{0,35}$"
    },
    "passthrough": {
      "type": "object",
      "description": "Optional field, used to pass data through the websocket, which may be retrieved via the echo_req output field."
    }
  },
  "additionalProperties": false
}
