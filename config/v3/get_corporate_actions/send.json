{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Corporate actions for an open contract",
  "description": "Get corporate actions for an open contract in the user's portfolio",
  "properties": {
    "get_corporate_actions": {
      "type": "integer",
      "description":"Must be 1",
      "pattern": "^1$",
      "required": "1"
    },
    "symbol": {
      "type": "string",
      "description":"Equity's symbol. Example: MSFT,GOOG etc",
      "pattern":"^\\w{1,10}$"
    },
    "start": {
      "type": "string",
      "description": "Start date (epoch or YYYY-MM-DD).",
      "pattern": "^(\\d{4}-\\d{1,2}-\\d{1,2}|\d{10})$"
    },
    "end": {
      "type": "string",
      "description": "Optional start date (epoch or YYYY-MM-DD). Will default to 1 year range if not specified.",
      "pattern": "^(\\d{4}-\\d{1,2}-\\d{1,2}|\d{10})$"
    }
  },
  "additionalProperties": false
}
