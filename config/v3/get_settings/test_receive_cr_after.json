{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "echo_req": {
            "required": "1"
        },
        "get_settings": {
            "type": "object",
            "properties": {
                "country": {
                  "type": "string",
                  "pattern": "^Indonesia$",
                  "required": "1"
                },
                "salutation": {
                  "type": "string",
                  "pattern": "^Mr$",
                  "required": "1"
                },
                "is_authenticated_payment_agent": {
                  "type": "integer",
                  "pattern": "^0$",
                  "required": "1"
                },
                "country_code": {
                  "type": "string",
                  "pattern": "^id$",
                  "required": "1"
                },
                "date_of_birth": {
                  "type": "integer",
                  "pattern": "^318124800$",
                  "required": "1"
                },
                "address_state": {
                  "type": "string",
                  "pattern": "^BT$",
                  "required": "1"
                },
                "address_postcode": {
                  "type": "string",
                  "pattern": "^123456$",
                  "required": "1"
                },
                "phone": {
                  "type": "string",
                  "pattern": "^1234567890$",
                  "required": "1"
                },
                "last_name": {
                  "type": "string",
                  "pattern": "^Pan$",
                  "required": "1"
                },
                "email": {
                  "type": "string",
                  "pattern": "test@binary.com",
                  "required": "1"
                },
                "address_line_2": {
                  "type": "string",
                  "pattern": "^Test Address Line 2$",
                  "required": "1"
                },
                "address_city": {
                  "type": "string",
                  "pattern": "^Test City$",
                  "required": "1"
                },
                "address_line_1": {
                  "type": "string",
                  "pattern": "^Test Address Line 1$",
                  "required": "1"
                },
                "client_tnc_status": {
                    "type": "string",
                    "pattern": "^Version \\d{2} \\d{4}-\\d{2}-\\d{2}$",
                    "required": "1"
                },
                "first_name": {
                  "type": "string",
                  "pattern": "^Peter$",
                  "required": "1"
                }
            },
            "required": "1"
        },
        "msg_type":{
            "type": "string",
            "pattern": "^get_settings$",
            "required": "1"
        }
    }
}
