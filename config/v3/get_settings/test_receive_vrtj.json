{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "get_settings": {
            "type": "object",
            "required": 1,
            "properties": {
                "email": {
                    "type": "string",
                    "required": 1
                },
                "country": {
                    "type": "string",
                    "required": 1
                },
                "country_code": {
                    "type": "string",
                    "pattern": "^jp$",
                    "required": 1
                },
                "jp_account_status": {
                    "type": "object",
                    "properties": {
                        "status": {
                            "type": "string",
                            "pattern": "activated"
                        }
                    }
                }
            }
        },
        "echo_req": {
            "required": 1
        },
        "msg_type":{
            "type": "string",
            "pattern": "^get_settings$",
            "required": 1
        }
    }
}
