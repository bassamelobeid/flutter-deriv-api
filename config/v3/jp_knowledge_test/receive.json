{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Knowledge Test Receive",
  "description": "A message with Knowledge Test",
  "properties": {
    "echo_req": {
      "title":"Echo request",
      "description":"Echo of the request made"
    },
    "jp_knowledge_test": {
      "type": "object",
      "title": "Japan Knowledge Test",
      "description": "Knowledge Test for Japan real money account opening",
      "properties": {
        "test_taken_time": {
          "type": "integer",
          "description": "Test taken time in epoch"
        }
      }
    },
    "msg_type": {
      "type": "string",
      "description": "jp_knowledge_test"
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field send in request to map to response, present only when request contains req_id"
    }
  },
  "required": [
    "echo_req",
    "jp_knowledge_test",
    "msg_type"
  ]
}
