{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Ping Receive",
  "properties": {
    "echo_req": {
      "type": "object",
      "required": "1",
      "properties": {
	    "ping": {
	    	"type": "string",
        "pattern": "^1$"
	    }
      }
    },
    "ping": {
    	"type": "string",
      "pattern": "^pong$"
    },
    "msg_type": {
        "type": "string",
        "pattern": "^ping$",
        "required": "1"
    }
  }
}
