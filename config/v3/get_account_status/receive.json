{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Get Account Status Receive",
  "description": "A message with Account Status",
  "properties": {
    "echo_req": {
      "title":"Echo request",
      "description":"Echo of the request made"
    },
    "get_account_status": {
      "type": "array",
      "title": "Account status array",
      "items": {
        "type": "object",
        "title": "Account status",
        "description": "status code with last modified date",
        "properties": {
          "code": {
            "enum": ["active", "age_verification", "can_authenticate", "cashier_locked", "disabled", "ok", "unwelcome", "withdrawal_locked", "ukgc_funds_protection", "tnc_approval", "jp_knowledge_test_pending", "jp_knowledge_test_fail", "jp_activation_pending"],
            "description": "status code"
          },
          "datetime": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}$",
            "description": "Last modified datetime of status.",
          },
        }
      }
    },
    "msg_type": {
      "type": "string",
      "description": "get_account_status"
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field send in request to map to response, present only when request contains req_id"
    }
  },
  "required": [
    "echo_req",
    "get_account_status",
    "msg_type"
  ]
}
