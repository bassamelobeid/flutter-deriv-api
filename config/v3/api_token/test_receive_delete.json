{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "api_token": {
      "type": "object",
      "properties": {
        "tokens": {
          "type": "array",
          "items": {
              "type": "null"
            },
            "required": "1"
        },
        "delete_token": {
          "type":"integer",
          "pattern": "^1$",
          "required": "1"
        }
      },
      "required": "1"
    },
    "echo_req": {
        "required": "1"
    },
    "msg_type": {
      "type": "string",
      "pattern": "^api_token$",
      "required": "1"
    }
  }
}
