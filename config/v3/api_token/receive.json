{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "API Token Response",
  "description": "",
  "properties": {
    "api_token": {
      "type": "object",
      "properties": {
        "tokens": {
          "type": "array",
          "description": "API tokens"
        },
        "new_token": {
          "type":"integer",
          "pattern": "^1$",
          "description": "Token created."
        },
        "delete_token": {
          "type":"integer",
          "pattern": "^1$",
          "description": "Token deleted."
        }
      }
    },
    "echo_req": {
      "title": "Echo request",
      "description": "Echo of the request made"
    },
    "msg_type": {
      "type": "string",
      "description": "transfer_between_accounts"
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field send in request to map to response if req_id is send in request"
    }
  },
  "required": [
    "echo_req",
    "msg_type"
  ]
}
