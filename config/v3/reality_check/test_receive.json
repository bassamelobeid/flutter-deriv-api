{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
        "required": "1"
    },
    "reality_check": {
      "type": "object",
      "properties": {
        "start_time": {
          "type": "integer",
          "required": "1"
        },
        "loginid": {
          "type": "string",
          "pattern": "^MLT\\d+$",
          "required": "1"
        },
        "currency": {
          "type": ["string", "null"],
          "pattern": "^(USD|GBP|EUR|AUD)?$",
          "required": "1"
        },
        "buy_count": {
          "type": "integer",
          "required": "1"
        },
        "buy_amount": {
          "type": "number",
          "required": "1"
        },
        "sell_count": {
          "type": "integer",
          "required": "1"
        },
        "sell_amount": {
          "type": "number",
          "required": "1"
        },
        "potential_profit": {
          "type": "number",
          "required": "1"
        },
        "open_contract_count": {
          "type": "integer",
          "required": "1"
        }
      },
      "required": "1"
    },
    "msg_type": {
      "type": "string",
      "pattern": "^reality_check$",
      "required": "1"
    }
  }
}
