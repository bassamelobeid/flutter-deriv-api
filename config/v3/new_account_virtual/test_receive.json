{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "echo_req": {
    },
    "new_account_virtual": {
      "type": "object",
      "properties": {
        "client_id": {
          "type": "string",
          "pattern": "^VRTC\\d+$"
        },
        "email": {
          "type": "string",
          "pattern": "^test@binary.com$"
        },
        "currency": {
          "type": "string",
          "pattern": "^USD$"
        },
        "balance": {
          "type": "number",
          "pattern": "^10000$"
        },
        "oauth_token": {
        }
      },
      "required": [
        "client_id",
        "email",
        "currency",
        "balance",
        "oauth_token"
      ]
    },
    "msg_type": {
    },
    "req_id": {
    }
  },
  "required": [
    "echo_req",
    "msg_type",
    "new_account_virtual"
  ]
}
