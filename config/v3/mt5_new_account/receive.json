{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Create MT5 account Receive",
  "description": "Create MT5 account Receive",
  "properties": {
    "echo_req": {
      "title":"Echo request",
      "description":"Echo of the request made"
    },
    "mt5_new_account": {
      "type": "object",
      "title": "New MT5 account",
      "description": "New MT5 account details",
      "properties": {
        "login": {
          "type": "string",
          "description": "Login of new MT5 account"
        },
        "account_type": {
          "enum": ["demo", "real_money"],
          "description": "landing company full name"
        },
        "balance": {
          "type": "string",
          "description": "Account balance"
        }
      }
    },
    "msg_type": {
      "type": "string",
      "description": "mt5_new_account"
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field send in request to map to response, present only when request contains req_id"
    }
  },
  "required": [
    "echo_req",
    "msg_type"
  ]
}
