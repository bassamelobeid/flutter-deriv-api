{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Top Up Virtual Response",
  "description": "",
  "properties": {
    "topup_virtual": {
      "type": "object",
      "properties": {
        "currency": {
          "type": "string",
          "description": "Top up currency string"
        },
        "amount": {
          "type": "number",
          "description": "Top up amount"
        }
      }
    },
    "echo_req": {
      "title": "Echo request",
