{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Passkeys register (response)",
    "description": "Return the created passkey info",
    "type": "object",
    "required": [
        "echo_req",
        "msg_type"
    ],
    "properties": {
        "passkeys_register": {
            "title": "passkeys_register_options",
            "description": "Wrapper of the response.",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "created_at": {
                    "description": "The epoch timestamp when the passkey was created.",
                    "type": "integer"
                },
                "id": {
                    "description": "The system id of the stored passkey.",
                    "type": "integer"
                },
                "last_used": {
                    "description": "The epoch timestamp that the key was last used to authenticate the user.",
                    "type": "integer"
                },
                "name": {
                    "description": "The descriptive name of the passkey.",
                    "type": "string"
                },
                "passkey_id": {
                    "description": "The id of the passkey credential.",
                    "type": "string"
                },
                "stored_on": {
                    "description": "The name of the device where the credential is stored on.",
                    "type": "string"
                }
            }
        },
        "echo_req": {
            "description": "Echo of the request made.",
            "type": "object"
        },
        "msg_type": {
            "description": "Action name of the request made.",
            "type": "string",
            "enum": [
                "passkeys_register"
            ]
        },
        "req_id": {
            "description": "Optional field sent in request to map to response, present only when request contains `req_id`.",
            "type": "integer"
        }
    }
}
