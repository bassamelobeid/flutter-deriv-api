{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Get Available Accounts (response)",
    "description": "Types of accounts available to be created.",
    "type": "object",
    "required": [
        "echo_req",
        "msg_type"
    ],
    "properties": {
        "available_accounts": {
            "title": "available_accounts",
            "description": "Returns list of accounts that are available to be created - limited to wallets and can be extended",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "wallets"
            ],
            "properties": {
                "wallets": {
                    "description": "Wallet account types that are available to be created",
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "required": [
                            "account_type",
                            "currency",
                            "landing_company"
                        ],
                        "properties": {
                            "account_type": {
                                "description": "Account type of wallet",
                                "type": "string",
                                "enum": [
                                    "doughflow",
                                    "crypto",
                                    "paymentagent",
                                    "paymentagent_client",
                                    "p2p"
                                ]
                            },
                            "currency": {
                                "description": "Currency of wallet",
                                "type": "string"
                            },
                            "landing_company": {
                                "description": "Landing Company of wallet.",
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "echo_req": {
            "description": "Echo of the request made.",
            "type": "object"
        },
        "msg_type": {
            "description": "Action name of the request made.",
            "type": "string",
            "enum": [
                "available_accounts"
            ]
        },
        "req_id": {
            "description": "Optional field sent in request to map to response, present only when request contains `req_id`.",
            "type": "integer"
        }
    }
}
